  object InsertBillHistory: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'insert into oneyearlifeline'
      ' (range,'
      '  accountnumber,'
      '  kilowatthour,'
      '  lifelinediscount,'
      '  lifelinesubs,'
      '  billmonth)'
      'select'
      '    Case'
      '    when kilowatthour between 0 and 15 then   '#39'1. 0-15'#39
      '    when kilowatthour = 16 THEN               '#39'2. 16'#39
      '    when kilowatthour = 17 THEN               '#39'3. 17'#39
      '    when kilowatthour = 18 THEN               '#39'4. 18'#39
      '    when kilowatthour = 19 THEN               '#39'5. 19'#39
      '    when kilowatthour = 20 THEN               '#39'6. 20'#39
      '    when kilowatthour > 20 THEN               '#39'7. Above 20'#39
      '    ELSE '#39#39
      '    END'
      '    AS RANGERDER,'
      '    accountnumber,'
      '    kilowatthour,'
      
        '    if(LifelineDiscSubs < 0 and kilowatthour <= 20,LifelineDiscS' +
        'ubs,0) lifelinediscount,'
      
        '    if(LifelineDiscSubs >= 0,LifelineDiscSubs,0) lifelinesubsidy' +
        ','
      '    billmonth'
      'from billhistory'
      
        'where year(concat(substring(billmonth,3,2),'#39'-'#39',substring(billmon' +
        'th,1,2),'#39'-01'#39')) = :PYear and '
      '      ratecode like :ratecode and'
      '      kilowatthour > 0')
    Left = 126
    Top = 167
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'PYear'
      end
      item
        DataType = ftUnknown
        Name = 'ratecode'
      end>
  end
