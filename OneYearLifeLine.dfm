  object LifeLine: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'SELECT RANGE,'
      '       CASE '
      '       WHEN SUBSTRING(RANGE,1,1) = '#39'1'#39' THEN '#39'50%'#39
      '       WHEN SUBSTRING(RANGE,1,1) = '#39'2'#39' THEN '#39'40%'#39
      '       WHEN SUBSTRING(RANGE,1,1) = '#39'3'#39' THEN '#39'30%'#39
      '       WHEN SUBSTRING(RANGE,1,1) = '#39'4'#39' THEN '#39'20%'#39
      '       WHEN SUBSTRING(RANGE,1,1) = '#39'5'#39' THEN '#39'10%'#39
      '       WHEN SUBSTRING(RANGE,1,1) = '#39'6'#39' THEN '#39'05%'#39
      '       WHEN SUBSTRING(RANGE,1,1) = '#39'7'#39' THEN '#39'No Discount'#39
      '       else '#39#39
      '       end'
      '       as discount,'
      
        '       count(If(BillMonth = :Billmonth,ACCOUNTNUMBER,null)) DecA' +
        'ccounts,'
      '       sum(kilowatthour) kilowatthour,'
      '       sum(truncate(lifelinediscount,2)) lifelinediscount,'
      '       sum(truncate(lifelinesubs,2)) lifelinesubsidy'
      'from oneyearlifeline'
      'group by range'
      'order by range')
    OnCalcFields = LifeLineCalcFields
    Left = 64
    Top = 167
    ParamData = <
      item
        DataType = ftString
        Name = 'Billmonth'
        Value = '2007'
      end>
