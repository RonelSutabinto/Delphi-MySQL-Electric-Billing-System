  object NetCollection: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select a.area,'
      '       case a.area '
      '        when '#39'001'#39' then '#39'RIZAL'#39
      '        when '#39'002'#39' then '#39'SIBUTAD'#39
      '        when '#39'003'#39' then '#39'MUTIA'#39
      '        when '#39'004'#39' then '#39'LA LIBERTAD'#39
      '        when '#39'005'#39' then '#39'SERGIO OSME'#209'A'#39
      '        when '#39'006'#39' then '#39'DAPITAN'#39
      '        when '#39'007'#39' then '#39'PI'#209'AN'#39
      '        when '#39'008'#39' then '#39'POLANCO'#39
      '        when '#39'009'#39' then '#39'DIPOLOG NORTH'#39
      '        when '#39'010'#39' then '#39'DIPOLOG SOUTH'#39
      '        when '#39'011'#39' then '#39'KATIPUNAN'#39
      '        when '#39'012'#39' then '#39'ROXAS'#39
      '        when '#39'013'#39' then '#39'MANUKAN'#39
      '        when '#39'014'#39' then '#39'JOSE DALMAN'#39
      '        when '#39'015'#39' then '#39'SINDANGAN'#39
      '        when '#39'016'#39' then '#39'LEON B. POSTIGO'#39
      '        when '#39'017'#39' then '#39'SIAYAN'#39
      '        when '#39'018'#39' then '#39'SALUG'#39
      '        when '#39'019'#39' then '#39'LILOY'#39
      '        when '#39'020'#39' then '#39'LABASON'#39
      '        when '#39'021'#39' then '#39'TAMPILISAN'#39
      '        when '#39'022'#39' then '#39'GODOD'#39
      '        when '#39'023'#39' then '#39'GUTALAC'#39
      '        when '#39'024'#39' then '#39'DAPITAN / PI'#209'AN'#39
      '        when '#39'025'#39' then '#39'KALAWIT'#39
      '        when '#39#39'    then concat(a.accountnumber,'#39':'#39',a.datepaid)'
      '        else '#39#39' '
      '       end '
      '       as address,'
      '       sum(ifnull(a.Total,0)) Total,'
      ''
      '       sum(ifnull(a.VATDist,0)+'
      '           ifnull(a.VATDistDiscAmt,0)+'
      '           ifnull(a.VATGenCO,0)+'
      '           ifnull(a.VATGenCODiscAmt,0)+'
      '           ifnull(a.VATTransCO,0)+'
      '           ifnull(a.VATTransCODiscAmt,0)+'
      
        '           ifnull(a.VATSystemLossGenCO,0)+                      ' +
        '          '
      '           ifnull(a.VATSystemLossDiscAmt,0)) EVAT,'
      '           -- ifnull(a.VATDiscAmt,0)) EVAT,'
      ''
      
        '       sum(ifnull(a.VATDist,0)+ifnull(a.VATDistDiscAmt,0)) EVATD' +
        'ist,'
      
        '       sum(ifnull(a.VATGenCO,0)+ifnull(a.VATGenCODiscAmt,0)) EVA' +
        'TGenCO,'
      
        '       sum(ifnull(a.VATTransCO,0)+ifnull(a.VATTransCODiscAmt,0))' +
        ' EVATTransCO,'
      '       sum(ifnull(a.VATSystemLossGenCO,0)+           '
      '           ifnull(a.VATSystemLossDiscAmt,0)) EVATSystemLoss,'
      '       sum(ifnull(a.UCEC,0)) UCEC,'
      '       sum(ifnull(a.UCME,0)) UCME,'
      '       sum(ifnull(a.UCNPCSCC,0)) UCNPCSCC,'
      '       sum(ifnull(a.SystemLossCharge,0))+'
      '       sum(ifnull(a.OtherSystemLossCostAdj,0)) SystemLossCharge,'
      '       '
      '       '
      '       sum(ifnull(l.amount,0)) TR,'
      ''
      '       sum(ifnull(a.PKVRAmount,0)) PKVRAmount,'
      '       sum(ifnull(a.security,0)) mcc,'
      '       sum(ifnull(a.Total,0) - (ifnull(a.SystemLossCharge,0) + '
      
        '                                ifnull(a.OtherSystemLossCostAdj,' +
        '0) +'
      '                                '
      '                                -- ===================VAT===='
      '                                -- ifnull(a.VAT,0) + '
      '                                ifnull(a.VATDist,0)+'
      '                                ifnull(a.VATDistDiscAmt,0)+'
      '                                ifnull(a.VATGenCO,0)+'
      '                                ifnull(a.VATGenCODiscAmt,0)+'
      '                                ifnull(a.VATTransCO,0)+'
      '                                ifnull(a.VATTransCODiscAmt,0)+'
      
        '                                ifnull(a.VATSystemLossGenCO,0)+ ' +
        '                               '
      
        '                                ifnull(a.VATSystemLossDiscAmt,0)' +
        '+'
      '                                -- =========================='
      ''
      '                                ifnull(a.UCEC,0) + '
      '                                ifnull(a.UCME,0) + '
      '                                ifnull(a.UCNPCSCC,0) + '
      
        '                                ifnull(a.security,0) +          ' +
        '                     '
      
        '                                ifnull(l.amount,0)+             ' +
        '         '
      '                               '
      '                                ifnull(Fitall,0)+'
      '                                ifnull(RPTax,0)+'
      '                                ifnull(UCNPCSD,0)+'
      '                                ifnull(ICCrossSubsidyCharge,0)+'
      '                                ifnull(PARec,0)'
      '                                -- ifnull(a.PKVRAmount,0)'
      '                                ) ) totalNet,'
      ''
      '     sum(ifnull(Fitall,0)) Fitall,'
      '     sum(ifnull(RPTax,0))  RPT,'
      '     sum(ifnull(UCNPCSD,0)) UCNPCSCD, '
      '     sum(ifnull(ICCrossSubsidyCharge,0)) ICCS,'
      '     sum(ifnull(PARec,0)) PARec,  '
      '     sum(ifnull(VATmcc,0)) VATmcc, '
      ''
      
        '     concat(if(sum(if(d.occode='#39'PF01'#39',ifnull(l.amount,0),0))=0,'#39 +
        #39','
      
        '                                                              co' +
        'ncat('#39'PF01 '#39',sum(if(d.occode='#39'PF01'#39',ifnull(l.amount,0),0)),'#39'\r\n' +
        #39') ),'
      
        '            if(sum(if(d.occode='#39'P.RENTAL'#39',ifnull(l.amount,0),0))' +
        '=0,'#39#39','
      
        '                                                              co' +
        'ncat( '#39'P.RENT '#39',sum(if(d.occode='#39'P.RENTAL'#39',ifnull(l.amount,0),0)' +
        '),'#39'\r\n'#39') ),'
      
        '            if(sum(if(d.occode='#39'XRR'#39',ifnull(l.amount,0),0))=0,'#39#39 +
        ','
      
        '                                                              co' +
        'ncat( '#39'XRR '#39',sum(if(d.occode='#39'XRR'#39',ifnull(l.amount,0),0)),'#39'\r\n'#39 +
        ') ),'
      
        '            if(sum(if(d.occode='#39'PROM. NOTE'#39',ifnull(l.amount,0),0' +
        '))=0,'#39#39','
      
        '                                                              co' +
        'ncat( '#39'P.NOTE '#39',sum(if(d.occode='#39'PROM. NOTE'#39',ifnull(l.amount,0),' +
        '0)),'#39'\r\n'#39') ),'
      '            if(sum(if(d.occode<>'#39'PROM. NOTE'#39' AND'
      '                      d.occode<>'#39'XRR'#39' AND '
      '                      d.occode<>'#39'P.RENTAL'#39' AND'
      
        '                      d.occode<>'#39'PF01'#39',ifnull(l.amount,0),0))=0,' +
        #39#39','
      
        '                                                              co' +
        'ncat(ifnull(d.occode,'#39'OC '#39') ,sum(if(d.occode<>'#39'PROM. NOTE'#39' AND'
      
        '                                                                ' +
        '                  d.occode<>'#39'XRR'#39' AND '
      
        '                                                                ' +
        '                  d.occode<>'#39'P.RENTAL'#39' AND'
      
        '                                                                ' +
        '                  d.occode<>'#39'PF01'#39',ifnull(l.amount,0),0)) )  )  '
      '           ) breakDownOC'
      'from collection a'
      ''
      'left join ocledger l on l.code = a.Accountcode and'
      '                        l.tornumber = a.tellerornumber and'
      '                        l.billmonth = a.billmonth  '
      'left join ocdetail d on d.idOCDetail=l.idOCDetail'
      ''
      '-- ========================================'
      
        '-- ========================================Temporalily replace t' +
        'his statement'
      '/*left join ocdetail d on d.code = a.accountcode -- and'
      
        '                        -- d.occode in ('#39'PF01'#39','#39'P.RENTAL'#39','#39'XRR'#39',' +
        #39'PROM. NOTE'#39')'
      'left join ocledger l on l.code = d.code and'
      '                        l.tornumber = a.tellerornumber and'
      '                        l.billmonth = a.billmonth*/'
      '-- ================================================'
      '-- ================================================'
      ''
      ''
      'where (a.datepaid between :datefrom and :dateto) and        '
      '      (a.billmonth is not null) '
      'group by a.area '
      'order by a.area')
    OnCalcFields = NetCollectionCalcFields
    Left = 128
    Top = 296
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'datefrom'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'dateto'
        Value = nil
      end>
