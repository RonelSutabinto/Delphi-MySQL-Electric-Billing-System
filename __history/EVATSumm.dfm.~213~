  object TotalEVAT: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select sum(VAT) EVAT,'
      '       sum(VATDist) EVATDist,'
      '       sum(VATGenCO) EVATGenCO,'
      '       sum(VATTransCO) EVATTransCO,'
      
        '       sum(VATSystemLossGenCO+VATSystemLossTransCO) EVATSystemLo' +
        'ss'
      '       from collection'
      'where (datepaid between :datefrom and :dateto) and'
      '      (billmonth is not null)')
    Left = 392
    Top = 48
    ParamData = <
      item
        DataType = ftString
        Name = 'datefrom'
        Value = '2005/12/01'
      end
      item
        DataType = ftString
        Name = 'dateto'
        Value = '2005/12/31'
      end>
