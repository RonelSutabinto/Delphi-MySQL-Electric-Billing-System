      'select sum(ifnull(a.Total,0)) Total,'
      '       sum(ifnull(a.VAT,0)+ifnull(a.VATDiscAmt,0)) EVAT,'
      
        '       sum(ifnull(a.VATDist,0)+ifnull(a.VATDistDiscAmt,0)) EVATD' +
