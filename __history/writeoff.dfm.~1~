  object FindOld: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select * from unpaidbills'
      'where year(concat(substring(billmonth,3,2),'#39'/'#39','
      '                  substring(billmonth,1,2),'#39'/01'#39')) = :myyear'
      'order by accountnumber, date')
    Left = 688
    Top = 310
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'myyear'
      end>
