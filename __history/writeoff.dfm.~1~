  object InsertUnpaidBills: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'Insert into unpaidbills'
      '( Document,'
      '  Code,'
      '  Area,'
      '  Book,'
      '  AccountNumber,'
      '  Date,'
      '  Name,'
      '  BillMonth,'
      '  DeleteRemarks,'
      '  RateCode,'
      '  Reading,'
      '  KilowattHour,'
      '  Amount,'
      '  UCNPCSD,'
      '  UCNPCSCC,'
      '  UCDUSCC,'
