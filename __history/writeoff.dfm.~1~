  object CreateWriteOff: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'CREATE TABLE  `zaneco`.`writeoff` ('
      '  `Entry` int(11) NOT NULL auto_increment,'
      '  `Document` varchar(15) default NULL,'
      '  `Code` double default NULL,'
      '  `Area` char(3) default NULL,'
      '  `Book` char(3) default NULL,'
      '  `AccountNumber` varchar(10) default NULL,'
      '  `Date` date default NULL,'
      '  `Name` varchar(30) default NULL,'
      '  `BillMonth` varchar(4) default NULL,'
      '  `DeleteRemarks` varchar(30) default NULL,'
      '  `RateCode` char(2) default NULL,'
      '  `Reading` double default NULL,'
      '  `KilowattHour` double default NULL,'
      '  `Amount` double default NULL,'
      '  `UCNPCSD` double default NULL,'
      '  `UCNPCSCC` double default NULL,'
      '  `UCDUSCC` double default NULL,'
      '  `UCME` double default NULL,'
      '  `UCETR` double default NULL,'
      '  `UCEC` double default NULL,'
      '  `UCCSR` double default NULL,'
      '  `TransformerRental` double default NULL,'
      '  `VATDiscAmt` double default '#39'0'#39','
      '  `VATDistDiscAmt` double default '#39'0'#39','
      '  `VATGenCoDiscAmt` double default '#39'0'#39','
      '  `VATTransCoDiscAmt` double default '#39'0'#39','
      '  `VATSystemLossDiscAmt` double default '#39'0'#39','
      '  `VATDist` double default '#39'0'#39','
      '  `VATGenCO` double default '#39'0'#39','
      '  `VATTransCO` double default '#39'0'#39','
      '  `VATSystemLossGenCO` double default '#39'0'#39','
      '  `VATSystemLossTransCO` double default '#39'0'#39','
      '  `VAT` double default '#39'0'#39','
      '  `WriteoffRef` varchar(15) NOT NULL,'
      '  PRIMARY KEY  (`Entry`),'
      '  KEY `UnpbCode` (`Code`),'
      '  KEY `codebm` (`Code`,`BillMonth`),'
      '  KEY `BillNumber` (`Document`),'
      '  KEY `Index_5` (`Name`,`BillMonth`)'
      ') ENGINE=InnoDB DEFAULT CHARSET=latin1;')
    Left = 750
    Top = 310
  end
