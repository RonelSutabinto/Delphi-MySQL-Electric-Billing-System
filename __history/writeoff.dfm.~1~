      '  UCETR,'
      '  UCEC,'
      '  UCCSR,'
      '  TransformerRental,'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
      '  VATGenCoDiscAmt,'
      '  VATTransCoDiscAmt,'
      '  VATSystemLossDiscAmt,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO,'
      '  VAT)'
      'select'
      '  Document,'
      '  Code,'
      '  Area,'
      '  Book,'
      '  AccountNumber,'
      '  Date,'
      '  Name,'
      '  BillMonth,'
      '  DeleteRemarks,'
      '  RateCode,'
      '  Reading,'
      '  KilowattHour,'
      '  Amount,'
      '  UCNPCSD,'
      '  UCNPCSCC,'
      '  UCDUSCC,'
      '  UCME,'
      '  UCETR,'
      '  UCEC,'
      '  UCCSR,'
      '  TransformerRental,'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
      '  VATGenCoDiscAmt,'
      '  VATTransCoDiscAmt,'
      '  VATSystemLossDiscAmt,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO,'
      '  VAT'
      'from writeoff'
      'where accountnumber =  :accountnumber or '
      '      writeoffref = :writeoffref')
    Left = 337
    Top = 312
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'AccountNumber'
      end
      item
        DataType = ftUnknown
        Name = 'writeoffref'
      end>
  end
  object DeleteARLedger: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'delete from arledger using arledger, writeoff'
      'where (writeoff.accountnumber = :accountnumber or'
      '       writeoff.writeoffref = :writeoffref) and'
      '       writeoff.code = arledger.code and'
      '       writeoff.billmonth = arledger.billmonth and'
      '       writeoff.amount = arledger.credit and'
      '       arledger.collectorcode = writeoff.writeoffref')
    Left = 337
    Top = 344
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'accountnumber'
      end
      item
        DataType = ftUnknown
        Name = 'writeoffref'
      end>
  end
