      '  VATSystemLossDiscAmt,'
      '  VAT,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO )'
      'select '
      '  Code,'
      '  :postingdate,'
      '  OEBRNumber,'
      '  BillMonth,'
      '  PresentReadingKWH,'
      '  KilowattHour,'
      '  VAT+VATDiscAmt,'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
      '  VATGenCODiscAmt,'
      '  VATTransCODiscAmt,'
      '  VATSystemLossDiscAmt,'
      '  VAT,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO'
      'from TempBill'
      'where (OEBRNumber <> '#39#39') and'
      '      (CBook like :CBook) and'
      '       Truncate(VATDiscAmt,2) < 0.00')
    Left = 108
    Top = 152
    ParamData = <
      item
        DataType = ftString
        Name = 'postingdate'
        Value = ''
