  object VatLedgerUpdate: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'Replace into VatLedger'
      '( Code,'
      '  Date,'
      '  Document,'
      '  BillMonth,'
      '  Reading,'
      '  KilowattHour,'
      '  Debit,'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
