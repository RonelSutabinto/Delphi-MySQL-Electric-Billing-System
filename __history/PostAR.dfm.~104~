  object Q: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'Select CBook, Count(AccountNumber) TotalAccounts'
      'from tempbill where oebrnumber is not null '
      'group by CBook'
      'order by CBook')
    Left = 74
    Top = 24
  end
