  object PostARLedger: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'insert ignore into ARLedger'
      '( Code,'
      '  Date,'
      '  Document,'
      '  BillMonth,'
      '  Credit,'
      '  CollectorCode,'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
      '  VATGenCODiscAmt,'
      '  VATTransCODiscAmt,'
      '  VATSystemLossDiscAmt,'
      '  VAT,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO )'
      'select '
      '  Code,'
      '  PresentReadingDate,'
      '  OEBRNumber,'
      '  BillMonth,'
      '  AdjAmountApplied,'
      '  concat(OEBRNumber,'#39'-PKV'#39'),'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
      '  VATGenCODiscAmt,'
      '  VATTransCODiscAmt,'
      '  VATSystemLossDiscAmt,'
      '  VAT,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO'
      'from TempBill'
      'where oebrnumber <> '#39#39' and'
      '      CBook like :CBook and'
      '      AdjAmountApplied > 0')
    FetchRows = 1
    Left = 109
    Top = 56
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'CBook'
        Value = nil
      end>
  end
