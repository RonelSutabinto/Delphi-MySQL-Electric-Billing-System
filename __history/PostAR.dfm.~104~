  object InsertOCLedger: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'insert ignore into ocledger'
      '(idocdetail,'
      ' code,'
      ' dateentered,'
      ' billmonth,'
      ' billnumber,'
      ' amount)'
      ''
      'select '
      '   idocdetail,'
      '   code,'
      '   postdate,'
      '   billmonth,'
      '   billnumber,'
      '   amount '
      '   from'
      '     (select ocd.idocdetail,'
      '              t.code,'
      '              :postdate as postdate,'
      '              t.billmonth,'
      '              t.oebrnumber as billnumber,'
      '              t.ocamount1 as amount'
      '              from TempBill t'
      '               left join master m on (t.code = m.code)'
      
        '                 left join ocdetail ocd on (t.code = ocd.code an' +
        'd trim(t.occode1) = trim(ocd.occode))'
      
        '                 where T.CBook like :CBook and t.ocamount1 <> 0 ' +
        'and'
      '                       T.OEBRNumber <> '#39#39' '
      ''
      '      union'
      ''
      '      select ocd.idocdetail,'
      '              t.code,'
      '              :postdate as postdate,'
      '              t.billmonth,'
      '              t.oebrnumber as billnumber,'
      '              t.ocamount2'
      '              from TempBill t'
      '               left join master m on (t.code = m.code)'
      
        '                 left join ocdetail ocd on (t.code = ocd.code an' +
        'd trim(t.occode2) = trim(ocd.occode))'
      
        '                 where T.CBook like :CBook and t.ocamount2 <> 0 ' +
        'and'
      '                       T.OEBRNumber <> '#39#39' '
      '      union'
      ''
      '      select ocd.idocdetail,'
      '              t.code,'
      '              :postdate as postdate,'
      '              t.billmonth,'
      '              t.oebrnumber as billnumber,'
      '              t.ocamount3'
      '              from TempBill t'
      '               left join master m on (t.code = m.code)'
      
        '                 left join ocdetail ocd on (t.code = ocd.code an' +
        'd trim(t.occode3) = trim(ocd.occode))'
      
        '                 where T.CBook like :CBook and t.ocamount3 <> 0 ' +
        'and'
      '                       T.OEBRNumber <> '#39#39') '
      'tempoc')
    Left = 72
    Top = 224
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'postdate'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'CBook'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'postdate'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'CBook'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'postdate'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'CBook'
        Value = nil
      end>
  end
  object UpdateOCLedger: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'update ocledger, collection set '
      '   ocledger.tornumber = collection.tellerornumber, '
      '   ocledger.datepaid = collection.datepaid'
      'where ocledger.billnumber = collection.tellerornumber')
    Left = 109
    Top = 224
