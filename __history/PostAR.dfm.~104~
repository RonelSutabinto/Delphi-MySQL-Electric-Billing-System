  object MasterUpdateQuery: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'Update Master, TempBill set'
      '    Master.FeedBackCode              = TempBill.FeedBackCode,'
      
        '    Master.PreviousReadingDate       = TempBill.PreviousReadingD' +
        'ate,'
      
        '    Master.PresentReadingDate        = TempBill.PresentReadingDa' +
        'te,'
      
        '    Master.PreviousReadingKWH        = TempBill.PreviousReadingK' +
        'WH,'
      
        '    Master.PresentReadingKWH         = TempBill.PresentReadingKW' +
        'H,'
      '    Master.Diff                      = TempBill.Diff,'
      '    Master.Demand                    = TempBill.Demand,'
      '    Master.CM                        = '#39'0'#39','
      '    Master.CMMultiplier              = TempBill.CMMultiplier,'
      
        '    Master.CMPreviousReadingKWH      = TempBill.CMPreviousReadin' +
        'gKWH,'
      
        '    Master.CMPresentReadingKWH       = TempBill.CMPresentReading' +
        'KWH,'
      '    Master.CMKilowattHour            = TempBill.CMKilowattHour,'
      '    Master.CMDemand                  = TempBill.CMDemand,'
      '    Master.KilowattHour              = TempBill.KilowattHour,'
      '    Master.KilowattUsed              = TempBill.KilowattUsed,'
      
        '    Master.GenSysCharge              = TempBill.GenSysCharge,   ' +
        '       '
      '    Master.HostCommCharge            = TempBill.HostCommCharge,'
      '    Master.ForexCharge               = TempBill.ForexCharge,'
      
        '    Master.TCDemandCharge            = TempBill.TCDemandCharge, ' +
        '       '
      
        '    Master.TCTransSystemCharge       = TempBill.TCTransSystemCha' +
        'rge,   '
      
        '    Master.SystemLossCharge          = TempBill.SystemLossCharge' +
        ',       '
      '    Master.DCDemandCharge            = TempBill.DCDemandCharge,'
      
        '    Master.DCDistributionCharge      = TempBill.DCDistributionCh' +
        'arge,'
      '    Master.SCRetCustCharge           = TempBill.SCRetCustCharge,'
      
        '    Master.SCSupplySysCharge         = TempBill.SCSupplySysCharg' +
        'e,'
      
        '    Master.MCRetailCustCharge        = TempBill.MCRetailCustChar' +
        'ge,'
      '    Master.MCSystemCharge            = TempBill.MCSystemCharge,'
      
        '    Master.UCNPCStrandedDebts        = TempBill.UCNPCStrandedDeb' +
        'ts,'
      
        '    Master.UCNPCStrandedContCost     = TempBill.UCNPCStrandedCon' +
        'tCost,'
      
        '    Master.UCDUStrandedContCost      = TempBill.UCDUStrandedCont' +
        'Cost,'
      '    Master.UCME                      = TempBill.UCME,'
      
        '    Master.UCEqTaxesAndRoyalties     = TempBill.UCEqTaxesAndRoya' +
        'lties,'
      '    Master.UCEC                      = TempBill.UCEC,'
      
        '    Master.UCCrossSubRemoval         = TempBill.UCCrossSubRemova' +
        'l,'
      
        '    Master.ICCrossSubsidyCharge      = TempBill.ICCrossSubsidyCh' +
        'arge,'
      '    Master.PowerActRateRed           = TempBill.PowerActRateRed,'
      
        '    Master.LifelineDiscSubs          = TempBill.LifelineDiscSubs' +
        ',          '
      '    Master.LoanCondo                 = TempBill.LoanCondo,'
      
        '    /* Master.TransformerRental       = TempBill.TransformerRent' +
        'al, */'
      '    Master.TotalBill                 = TempBill.TotalBill,'
      '    Master.BillNumber                = TempBill.BillNumber,'
      '    Master.OEBRNumber                = TempBill.OEBRNumber,'
      '    Master.BillMonth                 = TempBill.BillMonth,'
      '    Master.VATDiscAmt                = TempBill.VATDiscAmt,'
      '    Master.VATDistDiscAmt            = TempBill.VATDistDiscAmt,'
      '    Master.VATGenCODiscAmt           = TempBill.VATGenCODiscAmt,'
      
        '    Master.VATTransCODiscAmt         = TempBill.VATTransCODiscAm' +
        't,'
      
        '    Master.VATSystemLossDiscAmt      = TempBill.VATSystemLossDis' +
        'cAmt,'
      '    Master.VATmcc                    = TempBill.VATmcc,'
      '    Master.VAT                       = TempBill.VAT,'
      '    Master.VATDist                   = TempBill.VATDist,'
      '    Master.VATGenCO                  = TempBill.VATGenCO,'
      '    Master.VATTransCO                = TempBill.VATTransCO,'
      
        '    Master.VATSystemLossGenCO        = TempBill.VATSystemLossGen' +
        'CO,'
      
        '    Master.VATSystemLossTransCO      = TempBill.VATSystemLossTra' +
        'nsCO,'
      
        '    Master.PrevYearAdjPowerCost      = TempBill.PrevYearAdjPower' +
        'Cost,'
      
        '    Master.SysLossUnderRecov         = TempBill.SysLossUnderReco' +
        'v,'
      '    Master.MCC                       = TempBill.MCC,'
      '    Master.DiffBillPerKwhr           = TempBill.DiffBillPerKwhr,'
      '    Master.DiffBillPerKW             = TempBill.DiffBillPerKW,'
      '    Master.DiffBillPerCust           = TempBill.DiffBillPerCust,'
      '    Master.PKVROebrnumber            = TempBill.PKVROebrnumber,'
      '    Master.PKVRBillMonth             = TempBill.PKVRBillMonth,'
      '    Master.PKVRAmount                = TempBill.PKVRAmount,'
      
        '    Master.SCDisc                    = TempBill.SCDisc,         ' +
        '        '
      '    Master.SCAmt4Disc                = TempBill.SCAmt4Disc,'
      '    Master.RPTax                     = TempBill.RPTax,'
      '    Master.RateOrder                 = TempBill.RateOrder,'
      '    Master.Fitall                    = TempBill.Fitall,'
      '    Master.PARec                     = TempBill.PARec,'
      '    Master.VATparec                  = TempBill.VATparec,'
      '    Master.OtherGenRateAdj           = TempBill.OtherGenRateAdj,'
      
        '    Master.OtherTransCostAdj         = TempBill.OtherTransCostAd' +
        'j,'
      
        '    Master.OtherTransDemandCostAdj   = TempBill.OtherTransDemand' +
        'CostAdj,'
      
        '    Master.OtherSystemLossCostAdj    = TempBill.OtherSystemLossC' +
        'ostAdj,'
      
        '    Master.OtherLifelineRateCostAdj  = TempBill.OtherLifelineRat' +
        'eCostAdj,'
      
        '    Master.OtherSeniorCitizenRateAdj = TempBill.OtherSeniorCitiz' +
        'enRateAdj,'
      '    Master.referenceAtmNo            = TempBill.referenceAtmNo'
      '   '
      '    '
      'where Master.code = TempBill.code and'
      '     TempBill.CBook like :CBook and'
      '     TempBill.OEBRNumber <> '#39#39)
    FetchRows = 1
    Left = 74
    Top = 56
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'CBook'
        Value = nil
      end>
