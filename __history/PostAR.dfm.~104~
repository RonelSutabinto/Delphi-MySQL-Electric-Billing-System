  object ARUpdateQuery: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'Replace into ARLedger'
      '( Code,'
      '  Date,'
      '  Document,'
      '  BillMonth,'
      '  Reading,'
      '  KilowattHour,'
      '  Debit,'
      '  UCNPCSD,'
      '  UCNPCSCC,'
      '  UCDUSCC,'
      '  UCME,'
      '  UCETR,'
      '  UCEC,'
      '  UCCSR,'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
      '  VATGenCODiscAmt,'
      '  VATTransCODiscAmt,'
      '  VATSystemLossDiscAmt,'
      '  VAT,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO )'
      'select '
      '  Code,'
      '  :postingdate,'
      '  OEBRNumber,'
      '  BillMonth,'
      '  PresentReadingKWH,'
      '  KilowattHour,'
      '  TotalBill,'
      '  UCNPCStrandedDebts,'
      '  UCNPCStrandedContCost,'
      '  UCDUStrandedContCost,'
      '  UCME,'
      '  UCEqTaxesAndRoyalties,'
      '  UCEC,'
      '  UCCrossSubRemoval,'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
      '  VATGenCODiscAmt,'
      '  VATTransCODiscAmt,'
      '  VATSystemLossDiscAmt,'
      '  VAT,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO'
      'from TempBill'
      'where (OEBRNumber <> '#39#39') and'
      '      (CBook like :CBook) and'
      '       not exists '
      '       (select code from billhistory '
      '         where tempbill.code = billhistory.code and '
      '               tempbill.billmonth = billhistory.billmonth)')
    Left = 74
    Top = 88
    ParamData = <
      item
        DataType = ftString
        Name = 'postingdate'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'CBook'
        Value = nil
      end>
  end
  object UnPBUpdateQuery: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'insert into UnpaidBills'
      '( Code,'
      '  Document,'
      '  Area,'
      '  Book,'
      '  AccountNumber,'
      '  Date,'
      '  Name,'
      '  BillMonth,'
      '  RateCode,'
      '  Reading,'
      '  KilowattHour,'
      '  Amount,'
      '  UCNPCSD,'
      '  UCNPCSCC,'
      '  UCDUSCC,'
      '  UCME,'
      '  UCETR,'
      '  UCEC,'
      '  UCCSR,'
      '  /* TransformerRental, */'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
      '  VATGenCODiscAmt,'
      '  VATTransCODiscAmt,'
      '  VATSystemLossDiscAmt,'
      '  VATmcc,'
      '  VAT,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO,'
      '  SystemLossCharge,      '
      '  PKVRAmount,'
      '  SCDisc,                '
      '  RPTax,'
      '  RateOrder, '
      '  WRateCode,  '
      '  OtherGenRateAdj,'
      '  OtherTransCostAdj,'
      '  OtherTransDemandCostAdj,'
      '  OtherSystemLossCostAdj,'
      '  OtherLifelineRateCostAdj,'
      '  OtherSeniorCitizenRateAdj,'
      '  ICCrossSubsidyCharge,'
      '  Fitall,'
      '  referenceAtmNo,'
      '  PARec,'
      '  VATparec,'
      '  gram)'
      'select'
      '  tempbill.Code,'
      '  tempbill.OEBRNumber,'
      '  tempbill.Area,'
      '  tempbill.Book,'
      '  tempbill.AccountNumber,'
      '  :postingdate,'
      '  tempbill.Name,'
      '  tempbill.BillMonth,'
      '  tempbill.RateCode,'
      '  tempbill.PresentReadingKWH,'
      '  tempbill.KilowattHour,'
      '  tempbill.TotalBill - tempbill.AdjAmountApplied,'
      '  tempbill.UCNPCStrandedDebts,'
      '  tempbill.UCNPCStrandedContCost,'
      '  tempbill.UCDUStrandedContCost,'
      '  tempbill.UCME,'
      '  tempbill.UCEqTaxesAndRoyalties,'
      '  tempbill.UCEC,'
      '  tempbill.UCCrossSubRemoval,'
      '  /* tempbill.TransformerRental, */'
      '  tempbill.VATDiscAmt,'
      '  tempbill.VATDistDiscAmt,'
      '  tempbill.VATGenCODiscAmt,'
      '  tempbill.VATTransCODiscAmt,'
      '  tempbill.VATSystemLossDiscAmt,'
      '  tempbill.VATmcc,'
      '  tempbill.VAT,'
      '  tempbill.VATDist,'
      '  tempbill.VATGenCO,'
      '  tempbill.VATTransCO,'
      '  tempbill.VATSystemLossGenCO,'
      '  tempbill.VATSystemLossTransCO,'
      '  tempbill.SystemLossCharge,'
      '  tempbill.PKVRAmount,'
      '  tempbill.SCDisc,'
      '  tempbill.RPtax,'
      '  tempbill.RateOrder,  '
      '  tempbill.wratecode,  '
      '  tempbill.OtherGenRateAdj,'
      '  tempbill.OtherTransCostAdj,'
      '  tempbill.OtherTransDemandCostAdj,'
      '  tempbill.OtherSystemLossCostAdj,'
      '  tempbill.OtherLifelineRateCostAdj,'
      '  tempbill.OtherSeniorCitizenRateAdj,'
      '  tempbill.ICCrossSubsidyCharge,'
      '  tempbill.Fitall,'
      '  tempbill.referenceAtmNo,'
      '  tempbill.PARec,'
      '  tempbill.VATparec,'
      '  tempbill.gram'
      '  '
      'from TempBill'
      'where (tempbill.OEBRNumber <> '#39#39') and'
      '      (tempbill.CBook like :CBook) and'
      '       tempbill.TotalBill > tempbill.AdjAmountApplied and'
      '       not exists (select a.accountcode,'
      '                          a.billmonth,'
      '                          a.totalbill from collection a'
      '                   where a.accountcode = tempbill.code and'
      '                         a.billmonth = tempbill.billmonth and'
      '                         a.posted = '#39'2'#39' and'
      
        '                         truncate(a.totalbill,0) = truncate((tem' +
        'pbill.TotalBill - tempbill.AdjAmountApplied),0))')
    Left = 74
    Top = 120
    ParamData = <
      item
        DataType = ftString
        Name = 'postingdate'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'CBook'
        Value = nil
      end>
  end
