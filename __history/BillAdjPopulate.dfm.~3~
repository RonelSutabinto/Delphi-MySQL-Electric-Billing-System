  object AreaBook: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'SELECT A.area,'
      '       A.Book,'
      '       B.Address,'
      '       count(if(A.debit > 0,A.AccountNumber,null)) TAccount,'
      '       sum(A.debit) debit,'
      
        '       sum(if(period_diff(:billmonth, concat(substring(billmonth' +
        ',3,2),substring(billmonth,1,2)) ) > 0,credit,0)) AppliedAmount,'
      
        '       sum(if(A.billmonth = :billmonth1,credit,0)) AppliedAmount' +
        'ThisMonth'
      'from billadjledger a'
      'left join vicinity b on (a.area = b.area and a.book = b.book)'
      'group by A.area, A.book'
      'order by A.area, A.book')
    OnCalcFields = SummCalcFields
    Left = 331
    Top = 14
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'billmonth'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'billmonth1'
        Value = nil
      end>
    object StringField1: TStringField
      FieldName = 'area'
      Size = 3
    end
    object StringField2: TStringField
      FieldName = 'address'
      Origin = '.address'
      Size = 14
    end
    object LargeintField1: TLargeintField
      FieldName = 'TAccount'
      Origin = '.TAccount'
    end
    object FloatField1: TFloatField
      FieldName = 'debit'
      Origin = '.debit'
      currency = True
    end
    object FloatField2: TFloatField
      FieldName = 'AppliedAmount'
      Origin = '.AppliedAmount'
      currency = True
    end
    object FloatField3: TFloatField
      FieldName = 'AppliedAmountThisMonth'
      Origin = '.AppliedAmountThisMonth'
      currency = True
    end
    object CurrencyField1: TCurrencyField
      FieldKind = fkCalculated
      FieldName = 'totalapplied'
      Calculated = True
    end
    object CurrencyField2: TCurrencyField
      FieldKind = fkCalculated
      FieldName = 'balance'
      Calculated = True
    end
  end
