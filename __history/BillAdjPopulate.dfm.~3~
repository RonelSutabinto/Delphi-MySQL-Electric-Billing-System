  object GetBillHistory: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'insert into billadjledger'
      '(code,'
      ' Area,'
      ' Book,'
      ' Sequence,'
      ' AccountNumber,'
      ' Name,'
      ' BillMonth,'
      ' Debit,'
      ' billnumber,'
      ' kilowatthour,'
      ' postingdate)'
      'select'
      ' code,'
      ' Area,'
      ' Book,'
      ' Sequence,'
      ' AccountNumber,'
      ' Name,'
      ' BillMonth,'
      ' :Debit,'
      ' oebrnumber,'
      ' kilowatthour,'
      ' :postingdate'
      'from billhistory'
      'where billmonth = :billmonth and'
      '      ratecode = '#39'R'#39' and'
      '      kilowatthour between :kwhrfrom and :kwhrto'
      'order by area, book, accountnumber')
    Left = 160
    Top = 14
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Debit'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'postingdate'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'billmonth'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'kwhrfrom'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'kwhrto'
        Value = nil
      end>
  end
  object detail: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'SELECT ACCOUNTNUMBER,'
      '       NAME,'
      '       LPAD(idBillAdjLedger,6,'#39'0'#39') CreditMemo,'
      '       sum(debit) debit,'
      
        '       sum(if(period_diff(:billmonth, concat(substring(billmonth' +
        ',3,2),substring(billmonth,1,2)) ) > 0,credit,0)) AppliedAmount,'
      
        '       sum(if(billmonth = :billmonth1,credit,0)) AppliedAmountTh' +
        'isMonth'
      'from billadjledger'
      'where area = :area'
      'group by accountnumber'
      'order by accountnumber')
    OnCalcFields = detailCalcFields
    Left = 193
    Top = 16
    ParamData = <
      item
        DataType = ftString
        Name = 'billmonth'
        Value = '0608'
      end
      item
        DataType = ftString
        Name = 'billmonth1'
        Value = '0608'
      end
      item
        DataType = ftString
        Name = 'area'
        Value = '005'
      end>
    object detailACCOUNTNUMBER: TStringField
      FieldName = 'ACCOUNTNUMBER'
      Size = 10
    end
    object detailNAME: TStringField
      FieldName = 'NAME'
      Size = 30
    end
    object detailCreditMemo: TStringField
      FieldName = 'CreditMemo'
      Size = 6
    end
    object detaildebit: TFloatField
      FieldName = 'debit'
      currency = True
    end
    object detailAppliedAmount: TFloatField
      FieldName = 'AppliedAmount'
      currency = True
    end
    object detailAppliedAmountThisMonth: TFloatField
      FieldName = 'AppliedAmountThisMonth'
      currency = True
    end
    object detailbalance: TCurrencyField
      FieldKind = fkCalculated
      FieldName = 'balance'
      Calculated = True
    end
    object detailtotalapplied: TCurrencyField
      FieldKind = fkCalculated
      FieldName = 'totalapplied'
      Calculated = True
    end
  end
  object Summ: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'SELECT area,'
      '       case Area '
      '        when '#39'001'#39' then '#39'RIZAL'#39
      '        when '#39'002'#39' then '#39'SIBUTAD'#39
      '        when '#39'003'#39' then '#39'MUTIA'#39
      '        when '#39'004'#39' then '#39'LA LIBERTAD'#39
      '        when '#39'005'#39' then '#39'SERGIO OSME'#209'A'#39
      '        when '#39'006'#39' then '#39'DAPITAN'#39
      '        when '#39'007'#39' then '#39'PI'#209'AN'#39
      '        when '#39'008'#39' then '#39'POLANCO'#39
      '        when '#39'009'#39' then '#39'DIPOLOG NORTH'#39
      '        when '#39'010'#39' then '#39'DIPOLOG SOUTH'#39
      '        when '#39'011'#39' then '#39'KATIPUNAN'#39
      '        when '#39'012'#39' then '#39'ROXAS'#39
      '        when '#39'013'#39' then '#39'MANUKAN'#39
      '        when '#39'014'#39' then '#39'JOSE DALMAN'#39
      '        when '#39'015'#39' then '#39'SINDANGAN'#39
      '        when '#39'016'#39' then '#39'LEON B. POSTIGO'#39
      '        when '#39'017'#39' then '#39'SIAYAN'#39
      '        when '#39'018'#39' then '#39'SALUG'#39
      '        when '#39'019'#39' then '#39'LILOY'#39
      '        when '#39'020'#39' then '#39'LABASON'#39
      '        when '#39'021'#39' then '#39'TAMPILISAN'#39
      '        when '#39'022'#39' then '#39'GODOD'#39
      '        when '#39'023'#39' then '#39'GUTALAC'#39
      '        when '#39'024'#39' then '#39'DAPITAN / PI'#209'AN'#39
      '        when '#39'025'#39' then '#39'KALAWIT'#39
      '        else '#39#39' '
      '       end '
      '       as address,'
      '       count(if(debit > 0,AccountNumber,null)) TAccount,'
      '       sum(debit) debit,'
      
        '       sum(if(period_diff(:billmonth, concat(substring(billmonth' +
        ',3,2),substring(billmonth,1,2)) ) > 0,ifnull(truncate(credit,2),' +
        '0),0)) AppliedAmount,'
      
        '       sum(if(billmonth = :billmonth1,ifnull(truncate(credit,2),' +
        '0),0)) AppliedAmountThisMonth'
      'from billadjledger'
      'group by Area'
      'order by area')
    OnCalcFields = SummCalcFields
    Left = 259
    Top = 14
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'billmonth'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'billmonth1'
        Value = nil
      end>
    object Summarea: TStringField
      FieldName = 'area'
      Size = 3
    end
    object Summaddress: TStringField
      FieldName = 'address'
      Origin = '.address'
      Size = 14
    end
    object SummTAccount: TLargeintField
      FieldName = 'TAccount'
      Origin = '.TAccount'
    end
    object Summdebit: TFloatField
      FieldName = 'debit'
      Origin = '.debit'
      currency = True
    end
    object SummAppliedAmount: TFloatField
      FieldName = 'AppliedAmount'
      Origin = '.AppliedAmount'
      currency = True
    end
    object SummAppliedAmountThisMonth: TFloatField
      FieldName = 'AppliedAmountThisMonth'
      Origin = '.AppliedAmountThisMonth'
      currency = True
    end
    object Summtotalapplied: TCurrencyField
      FieldKind = fkCalculated
      FieldName = 'totalapplied'
      Calculated = True
    end
    object Summbalance: TCurrencyField
      FieldKind = fkCalculated
      FieldName = 'balance'
      Calculated = True
    end
  end
