//  btnPrint.Enabled :=
end;

procedure TfrmPowerSalesNu.FormShow(Sender: TObject);
begin
  dtpDateFrom.Date := StartOfTheMonth(Now);
  dtpDateTo.Date   := EndOfTheMonth(Now);
  eBillMonthChange(Sender);
end;

procedure TfrmPowerSalesNu.NxButton1Click(Sender: TObject);
begin
  qPowerSales.Close;
  qupowersalesbd.close;
  qPowerSalesLate.Close;
  if psSignatories.Active then psSignatories.Refresh
  else psSignatories.Open;

  if rbSummary.Checked then reptype    := 1;
  if rbUnbundled.Checked then reptype  := 2;
  if rbSubsidiary.Checked then reptype := 3;
  //qrLateSummary

  qPowerSalesLate.Close;
  if rbCurrent.Checked then
   begin

     qPowerSales.ParamByName('BM').Text      := eBillMonth.Text;
     qPowerSales.ParamByName('RT').AsInteger := reptype;
     qPowerSales.ParamByName('A').Text       := eArea.Text;
     qPowerSales.ParamByName('DF').AsDate    := dtpDateFrom.Date;
     qPowerSales.ParamByName('DT').AsDate    := dtpDateTo.Date;

     qPowerSales.Open;

     qupowersalesbd.ParamByName('BM').Text      := eBillMonth.Text;
     qupowersalesbd.ParamByName('RT').AsInteger := 1;
     qupowersalesbd.ParamByName('A').Text       := eArea.Text;
     qupowersalesbd.ParamByName('DF').AsDate    := dtpDateFrom.Date;
     qupowersalesbd.ParamByName('DT').AsDate    := dtpDateTo.Date;
     qupowersalesbd.Open;

     case reptype of
      1: QRCompositeReport1.Preview;
      2: qrCurrentUnbundled.Preview;
      3: qrCurrentSubsidiary.Preview;
     end;
   end
  else if rblate.Checked then
   begin
     qPowerSalesLate.ParamByName('DF').AsDate    := dtpDateFrom.Date;
     qPowerSalesLate.ParamByName('DT').AsDate    := dtpDateTo.Date;
     qPowerSalesLate.ParamByName('RT').AsInteger := reptype;
     qPowerSalesLate.ParamByName('A').Text       := eArea.Text;
     qPowerSalesLate.Open;

     qupowersalesbd.ParamByName('BM').Text      := eBillMonth.Text;
     qupowersalesbd.ParamByName('RT').AsInteger := 2;
     qupowersalesbd.ParamByName('A').Text       := eArea.Text;
     qupowersalesbd.ParamByName('DF').AsDate    := dtpDateFrom.Date;
     qupowersalesbd.ParamByName('DT').AsDate    := dtpDateTo.Date;
     qupowersalesbd.Open;
     qrlabel110.caption := 'UNBUNDLED POWERSALES REPORT (LATEBILLS)';
     case reptype of
      1: QRCompositeReport2.preview;
      2: qrLateUnbundled.Preview;
      3: qrLateSubsidiary.Preview;
     end;
   end;
end;

procedure TfrmPowerSalesNu.NxButton2Click(Sender: TObject);
begin
  if psSignatories.Active then psSignatories.Refresh
  else psSignatories.Open;
  if psSignatories.IsEmpty then psSignatories.Append
  else psSignatories.Edit;
  Panel7.Visible := True;

  frmPowerSalesNu.Width := 791;

end;

procedure TfrmPowerSalesNu.QRBand12BeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
  qrlLUFDMonth.Caption := 'For the Month of ' + RetrieveMonthName(dtpDateto.Date);
end;

procedure TfrmPowerSalesNu.QRBand16BeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
  qrlLSuFDMonth.Caption := 'For the Month of ' + RetrieveMonthName(dtpDateto.Date);
  if reptype = 4 then qrlLSuMunicipality.caption := '' else qrlLSuMunicipality.Caption := 'Municipality of ' + RetrieveMunicipalityName(eArea.Text);
end;

procedure TfrmPowerSalesNu.QRBand20BeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
  qrlLSFDMonth.Caption := 'Date coverage ' + FormatDateTime('MM/DD/YYYY',dtpDateFrom.Date) + ' - ' +
                                             FormatDateTime('MM/DD/YYYY',dtpDateTo.Date);
end;

procedure TfrmPowerSalesNu.QRBand21BeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
  qrlCSFDMonth.Caption := 'For the Month of ' + RetrieveMonthName(StrToDate(LeftStr(eBillMonth.Text,2)+'/1/'+RightStr(eBillMonth.Text,2)));
end;

procedure TfrmPowerSalesNu.QRBand4BeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
  qrlCUFDMonth.Caption := 'For the Month of ' + RetrieveMonthName(StrToDate(LeftStr(eBillMonth.Text,2)+'/1/'+RightStr(eBillMonth.Text,2)));
end;

procedure TfrmPowerSalesNu.QRBand8BeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
  qrlCSuFDMonth.Caption := 'For the Month of ' + RetrieveMonthName(StrToDate(LeftStr(eBillMonth.Text,2)+'/1/'+RightStr(eBillMonth.Text,2)));
  qrlCSuMunicipality.Caption := 'Municipality of ' + RetrieveMunicipalityName(eArea.Text);
end;

procedure TfrmPowerSalesNu.QRCompositeReport1AddReports(Sender: TObject);
begin
 with QRCompositeReport1.Reports do
  begin
   Add(qrCurrentSummary);
   Add(qrPowerSalesBD);
   Add(qrOTC);
   Add(QuickRep1);
   Add(QuickRep2);
   Add(qrOTC2);
  end;

end;

procedure TfrmPowerSalesNu.QRCompositeReport2AddReports(Sender: TObject);
begin
 with QRCompositeReport2.Reports do
  begin
   Add(qrLateSummary);
   Add(qrPowerSalesBD);
   Add(qrOTC);
   Add(QuickRep1);
   Add(QuickRep2);
   Add(qrOTC2);
  end;
end;

procedure TfrmPowerSalesNu.qupowersalesbdCalcFields(DataSet: TDataSet);
begin
 qupowersalesbdGtotal.asfloat :=    qupowersalesbdEnvironmental.asfloat +
                                    qupowersalesbdMissionary.asfloat +
                                    qupowersalesbdscc.asfloat +
                                    qupowersalesbdDistribution.asfloat +
                                    qupowersalesbdGenco.asfloat +
                                    qupowersalesbdTransco.asfloat +
                                    qupowersalesbdSysloss.asfloat +
                                    qupowersalesbdPantawid.asfloat +
                                    qupowersalesbdRFSC.asfloat +

                                    qupowersalesbdTAmount.asfloat +
                                    qupowersalesbdOTCharges.asfloat +
                                    qupowersalesbdriOUtotal.asfloat +
                                    qupowersalesbdFitall.AsFloat;
end;

procedure TfrmPowerSalesNu.rbLateClick(Sender: TObject);
begin
  pLateBill.Top    := 47;
  pCurrentBill.Top := 47;

  pCurrentBill.Visible := rbCurrent.Checked;

  panel2.Visible       := (rblate.checked) or (rbcurrent.checked);
  panel3.Visible       := (rblate.checked) or (rbcurrent.checked);

end;

procedure TfrmPowerSalesNu.rbSubsidiaryClick(Sender: TObject);
begin
  eArea.Enabled := rbSubsidiary.Checked;
  eBillMonthChange(Sender);
end;

function TfrmPowerSalesNu.RetrieveMonthName(Date: TDate): String;
begin
  Result := FormatDateTime('MMMM YYYY',Date);
end;

function TfrmPowerSalesNu.RetrieveMunicipalityName(pArea: string): string;
begin
  case strtoint(pArea) of
   1: Result := 'RIZAL';
   2: Result := 'SIBUTAD';
   3: Result := 'MUTIA';
   4: Result := 'LA LIBERTAD';
   5: Result := 'SERGIO OSMEÑA';
   6: Result := 'DAPITAN';
   7: Result := 'PIÑAN';
   8: Result := 'POLANCO';
   9: Result := 'DIPOLOG NORTH';
   10: Result := 'DIPOLOG SOUTH';
   11: Result := 'KATIPUNAN';
   12: Result := 'ROXAS';
   13: Result := 'MANUKAN';
   14: Result := 'JOSE DALMAN';
   15: Result := 'SINDANGAN';
   16: Result := 'LEON B. POSTIGO';
   17: Result := 'SIAYAN';
   18: Result := 'SALUG';
   19: Result := 'LILOY';
   20: Result := 'LABASON';
   21: Result := 'TAMPILISAN';
   22: Result := 'GODOD';
   23: Result := 'GUTALAC';
   24: Result := 'DAPITAN / PIÑAN';
   25: Result := 'KALAWIT';
  end;
end;

end.
