  object PrevDate: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select PresentReadingDate, '
      '       count(accountnumber) TAccount '
      'from master'
      'where conncode = '#39'1'#39' and'
      '      area = :area and '
      '      book = :book and '
      '      previousreadingdate is not null'
      'group by previousreadingdate'
      'order by taccount desc'
      ''
      '          ')
    Left = 152
    Top = 8
    ParamData = <
      item
        DataType = ftString
        Name = 'area'
        Value = '005'
      end
      item
        DataType = ftString
        Name = 'book'
        Value = '001'
      end>
    object PrevDatePresentReadingDate: TDateField
      FieldName = 'PresentReadingDate'
    end
    object PrevDateTAccount: TLargeintField
      FieldName = 'TAccount'
    end
  end
  object GetPKV: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select sum(truncate(debit,2)) debit, '
      '       sum(truncate(credit,2)) credit'
      'from billadjledger '
      'where code = :code and'
      '      area = :area and'
      '      book = :book')
    Left = 186
    Top = 9
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'code'
