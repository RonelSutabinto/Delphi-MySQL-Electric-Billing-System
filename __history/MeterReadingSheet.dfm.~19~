  object SCRead: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      
        'select *,DateDiff(expirationdate,:presentreadingdate) as stillva' +
        'lid'
      'from scdisc '
      '   where scode = :scode and'
      '         DateApplied <= :presentreadingdate'
      '   having stillvalid  > 0')
    Left = 704
    Top = 192
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'presentreadingdate'
      end
      item
        DataType = ftUnknown
        Name = 'scode'
      end
      item
        DataType = ftUnknown
        Name = 'presentreadingdate'
      end>
    object SCReadSCode: TFloatField
      FieldName = 'SCode'
    end
    object SCReadArea: TStringField
      FieldName = 'Area'
      FixedChar = True
      Size = 3
