  object TmpQuery: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select PresentReadingDate,'
      '       count(accountnumber) TAccount'
      'from master'
      'where conncode = '#39'1'#39' and'
      '      area = :area and'
      '      book = :book and'
      '      previousreadingdate is not null'
      'group by previousreadingdate'
      'order by taccount desc')
    Left = 376
    Top = 8
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'area'
      end
      item
        DataType = ftUnknown
        Name = 'book'
      end>
    object TmpQueryPresentReadingDate: TDateField
      FieldName = 'PresentReadingDate'
    end
    object TmpQueryTAccount: TLargeintField
      FieldName = 'TAccount'
    end
  end
