  object PKVR: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select * from billAdjLedger'
      'where code = :code and '
      '      area = :area and'
      '      book = :book and'
      '      credit > 0 and'
      '      exists (select * from pkvridx '
      '                 where pkvridx.code = :code and '
      '                       pkvridx.area = :area and'
      '                       pkvridx.book = :book) and'
      '      not exists (select * from pkvrledger'
      '                 where pkvrledger.code = :code and '
      '                       pkvrledger.area = :area and'
      '                       pkvrledger.book = :book and'
      
        '                       pkvrledger.billnumber = billadjledger.bil' +
        'lnumber)'
      'order by idbilladjledger'
      'limit 1')
    Left = 320
    Top = 8
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'code'
      end
      item
        DataType = ftUnknown
        Name = 'area'
      end
      item
        DataType = ftUnknown
        Name = 'book'
      end
      item
        DataType = ftUnknown
        Name = 'code'
      end
      item
        DataType = ftUnknown
        Name = 'area'
      end
      item
        DataType = ftUnknown
        Name = 'book'
      end
      item
        DataType = ftUnknown
        Name = 'code'
      end
      item
        DataType = ftUnknown
        Name = 'area'
      end
      item
        DataType = ftUnknown
        Name = 'book'
      end>
    object PKVRcode: TFloatField
      FieldName = 'code'
    end
    object PKVRArea: TStringField
      FieldName = 'Area'
      Size = 3
    end
    object PKVRBook: TStringField
      FieldName = 'Book'
      Size = 3
    end
    object PKVRSequence: TStringField
      FieldName = 'Sequence'
      Size = 4
    end
    object PKVRAccountNumber: TStringField
      FieldName = 'AccountNumber'
      Size = 10
    end
    object PKVRName: TStringField
      FieldName = 'Name'
      Size = 30
    end
    object PKVRBillMonth: TStringField
      FieldName = 'BillMonth'
      Size = 4
    end
    object PKVRDebit: TFloatField
      FieldName = 'Debit'
    end
    object PKVRcredit: TFloatField
      FieldName = 'credit'
    end
    object PKVRbalance: TFloatField
      FieldName = 'balance'
    end
    object PKVRbillnumber: TStringField
      FieldName = 'billnumber'
      Size = 10
    end
    object PKVRkilowatthour: TLargeintField
      FieldName = 'kilowatthour'
