  object GroupCollection: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select collection.collectorcode,'
      '       collector.name,'
      '       datepaid,'
      '       encodercode,'
      '       sundries,'
      '       sum(totalbill) totalbill,'
      '       count(accountnumber) totalaccount,'
      '       posted'
      'from collection'
      
        'left join collector on (collector.collectorcode = collection.col' +
        'lectorcode)'
      'group by datepaid,'
      '         collection.collectorcode,'
      '         encodercode'
      'order by datepaid desc,'
      '         collection.collectorcode,'
      '         encodercode')
    Left = 234
    Top = 176
    object GroupCollectioncollectorcode: TStringField
      FieldName = 'collectorcode'
      FixedChar = True
      Size = 3
    end
