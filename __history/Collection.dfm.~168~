  object GroupCollectionOption1: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select collection.collectorcode,'
      '       collector.name,'
      '       datepaid,'
      '       encodercode,'
      '       sundries,'
      '       sum(totalbill) totalbill,'
      '       count(accountnumber) totalaccount, '
      '       posted'
      'from collection '
      
        'left join collector on (collector.collectorcode = collection.col' +
        'lectorcode)'
      'where collection.collectorcode like :collectorcode and'
      '      datepaid between :datefrom and :dateto'
      'group by datepaid,'
      '         collection.collectorcode,'
      '         encodercode'
      'order by datepaid desc,'
      '         collection.collectorcode')
    Left = 234
    Top = 224
    ParamData = <
      item
        DataType = ftString
        Name = 'collectorcode'
        Value = ''
      end
      item
        DataType = ftString
        Name = 'datefrom'
        Value = ''
      end
      item
        DataType = ftString
        Name = 'dateto'
        Value = ''
      end>
    object StringField1: TStringField
      FieldName = 'collectorcode'
      FixedChar = True
      Size = 3
