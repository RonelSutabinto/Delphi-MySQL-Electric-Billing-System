        '       @Fitall_P   := @Fitall_P+if(STR_TO_DATE(concat(SUBSTR(c.B' +
        'illMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '                                   STR_TO_DATE(concat('#39'01/01/'#39',:' +
        'year),'#39'%m/%d/%Y'#39'),'
      
        '                                                ifnull(c.Fitall,' +
        '0),0) Fitall_P,'
      
        '       @ICCS_P     := @ICCS_P+ if(STR_TO_DATE(concat(SUBSTR(c.Bi' +
        'llMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '                                  STR_TO_DATE(concat('#39'01/01/'#39',:y' +
        'ear),'#39'%m/%d/%Y'#39'),'
      
        '                                                ifnull(c.ICCross' +
        'SubsidyCharge,0),0) ICCS_P,'
      
        '       @MCC_P      := @MCC_P + if(STR_TO_DATE(concat(SUBSTR(c.Bi' +
        'llMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '                                  STR_TO_DATE(concat('#39'01/01/'#39',:y' +
        'ear),'#39'%m/%d/%Y'#39'),'
      
        '                                                ifnull(c.securit' +
        'y,0),0) mcc_P,'
      ''
      
        '       @gram_P     := @gram_P + if(STR_TO_DATE(concat(SUBSTR(c.B' +
        'illMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '                                   STR_TO_DATE(concat('#39'01/01/'#39',:' +
        'year),'#39'%m/%d/%Y'#39'),'
      
        '                                                ifnull(c.gram,0)' +
        ',0) gram_P,'
      '       '
      ''
      
        '       @PF_P := @PF_P+if(STR_TO_DATE(concat(SUBSTR(c.BillMonth,1' +
        ',2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '                         STR_TO_DATE(concat('#39'01/01/'#39',:year),'#39'%m/' +
        '%d/%Y'#39'),'
      
        '                            if(d.occode='#39'PF01'#39' and m.conncode=1 ' +
        ',ifnull(o.amount,0),0),'
      '                      0) as PF01_P,'
      
        '       @PR_P := @PR_P+if(STR_TO_DATE(concat(SUBSTR(c.BillMonth,1' +
        ',2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '                         STR_TO_DATE(concat('#39'01/01/'#39',:year),'#39'%m/' +
        '%d/%Y'#39'),'
      
        '                            if(d.occode='#39'P.RENTAL'#39'  and m.connco' +
        'de=1,ifnull(o.amount,0),0),'
      '                      0) as PRENTAL_P,'
      
        '       @XR_P := @XR_P+if(STR_TO_DATE(concat(SUBSTR(c.BillMonth,1' +
        ',2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '                         STR_TO_DATE(concat('#39'01/01/'#39',:year),'#39'%m/' +
        '%d/%Y'#39'),'
      
        '                            if(d.occode='#39'XRR'#39'  and m.conncode=1,' +
        'ifnull(o.amount,0),0),'
      '                      0) as XRR_P,'
      
        '       @PN_P := @PN_P+if(STR_TO_DATE(concat(SUBSTR(c.BillMonth,1' +
        ',2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '                         STR_TO_DATE(concat('#39'01/01/'#39',:year),'#39'%m/' +
        '%d/%Y'#39'),'
      
        '                            if(d.occode='#39'PROM. NOTE'#39'  and m.conn' +
        'code=1,ifnull(o.amount,0),0),'
      '                      0) PNOTE_P,'
      
        '       @OC_P := @OC_P+if(STR_TO_DATE(concat(SUBSTR(c.BillMonth,1' +
        ',2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '                         STR_TO_DATE(concat('#39'01/01/'#39',:year),'#39'%m/' +
        '%d/%Y'#39'),'
      '                            if(d.occode<>'#39'PROM. NOTE'#39' AND'
      '                               d.occode<>'#39'XRR'#39' AND '
      '                               d.occode<>'#39'P.RENTAL'#39' AND'
      
        '                               d.occode<>'#39'PF01'#39'  and m.conncode=' +
        '1,ifnull(o.amount,0),0),'
      '                      0) OCcharge_P,'
      ''
      '     if(m.wratecode='#39'R'#39',1,'
      '                       if(m.wratecode='#39'L'#39',2,'
      
        '                                          if(m.wratecode='#39'H'#39',3,4' +
        '))) orderBy,'
      '     '
      '     :billmonth as bMonth'
      '     '
      ''
      'FROM Collection c'
      'inner join master m on (m.code = c.accountcode)'
      'left join ocledger o on o.code = c.Accountcode and'
      '                        o.tornumber = c.tellerornumber and'
      '                        o.billmonth = c.billmonth  '
      'left join ocdetail d on d.idOCDetail=o.idOCDetail'
      ''
      '   where c.DatePaid between :datefrom and :dateto '
      ''
      '-- GROUP BY c.Area, c.Book, c.BillMonth, m.wratecode'
      '-- ORDER BY c.Area, c.Book, c.BillMonth, m.wratecode'
      ''
      ') f')
    FetchAll = False
