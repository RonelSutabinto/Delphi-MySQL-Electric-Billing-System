  object TotalCollection: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'SELECT sundries ORNumber, '
      '       collection.collectorcode code, '
      '       collector.name name,'
      '       sum(totalbill) totalbill, '
      '       sum(ucec) ucec, '
      '       sum(ucme) ucme, '
      '       sum(vatgenco) GenCo, '
      '       sum(vattransco) TransCo, '
      '       sum(vatdist) Dist, '
      '       sum(vatsystemlossgenco) SystemLoss,'
      
        '       sum(totalbill - (ucec+ucme+vatgenco+vattransco+vatdist+va' +
        'tsystemlossgenco)) net,'
      '       sum(pkvramount) pkvramount'
      'FROM collection '
      
        'join collector on (collector.collectorcode = collection.collecto' +
        'rcode)'
      'where datepaid = :datepaid '
      'group by sundries'
      'order by sundries')
    Left = 56
    Top = 176
    ParamData = <
      item
        DataType = ftString
        Name = 'datepaid'
        Value = '2006/09/04'
      end>
    object TotalCollectionORNumber: TStringField
      FieldName = 'ORNumber'
      Size = 15
    end
    object TotalCollectioncode: TStringField
      FieldName = 'code'
      FixedChar = True
      Size = 3
    end
    object TotalCollectionname: TStringField
      FieldName = 'name'
      Size = 50
    end
    object TotalCollectiontotalbill: TFloatField
      FieldName = 'totalbill'
      currency = True
    end
    object TotalCollectionnet: TFloatField
      FieldName = 'net'
      currency = True
    end
    object TotalCollectionucec: TFloatField
      FieldName = 'ucec'
      currency = True
    end
    object TotalCollectionucme: TFloatField
      FieldName = 'ucme'
      currency = True
    end
    object TotalCollectionGenCo: TFloatField
      FieldName = 'GenCo'
      currency = True
    end
    object TotalCollectionTransCo: TFloatField
      FieldName = 'TransCo'
      currency = True
    end
    object TotalCollectionDist: TFloatField
      FieldName = 'Dist'
      currency = True
    end
    object TotalCollectionSystemLoss: TFloatField
      FieldName = 'SystemLoss'
      currency = True
    end
    object TotalCollectionpkvramount: TFloatField
