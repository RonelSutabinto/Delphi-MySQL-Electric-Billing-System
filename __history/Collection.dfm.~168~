  object CCollection: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'SELECT * FROM COLLECTION '
      'WHERE AREA = :AREA AND'
      '      BILLMONTH = :BILLMONTH AND'
      '      DATEPAID <= :DATEPAID AND'
      '      POSTED = '#39'2'#39
      'order by accountnumber   ')
