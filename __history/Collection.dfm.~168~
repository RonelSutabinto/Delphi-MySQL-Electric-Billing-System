  object CQtmp: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'set @PF :=0.00;'
      'set @PR :=0.00;'
      'set @XR :=0.00;'
      'set @OC :=0.00;'
      'set @PN :=0.00;'
      ''
      'set @VAT_      :=0.00;'
      'set @UCNPCSD_  :=0.00;'
      'set @UCEC_     :=0.00;'
      'set @UCME_     :=0.00;'
      'set @UCNPCSCC_ :=0.00;'
      'set @RPTax_    :=0.00;'
      'set @PARec_    :=0.00;'
      'set @Fitall_   :=0.00;'
      'set @ICCS_     :=0.00;'
      'set @MCC_      :=0.00;'
      'set @VATDist_  :=0.00;'
      'set @VATGenCO_ :=0.00;'
      'set @VATTransCO_ :=0.00;'
      'set @EVATSystemLoss_ :=0.00;'
      'set @gram_ := 0.00;'
      ''
      'set @PF_P :=0.00;'
      'set @PR_P :=0.00;'
      'set @XR_P :=0.00;'
      'set @OC_P :=0.00;'
      'set @PN_P :=0.00;'
      ''
      'set @VAT_P      :=0.00;'
      'set @UCNPCSD_P  :=0.00;'
      'set @UCEC_P     :=0.00;'
      'set @UCME_P     :=0.00;'
      'set @UCNPCSCC_P :=0.00;'
      'set @RPTax_P    :=0.00;'
      'set @PARec_P    :=0.00;'
      'set @Fitall_P   :=0.00;'
      'set @ICCS_P     :=0.00;'
      'set @MCC_P      :=0.00;'
      'set @VATDist_P  :=0.00;'
      'set @VATGenCO_P :=0.00;'
      'set @VATTransCO_P :=0.00;'
      'set @EVATSystemLoss_P :=0.00;'
      'set @gram_P:=0.00;'
      ''
      'set @cnt := 0;'
      'set @cnt_P:=0;'
      ''
      ''
      ''
      ''
      'Select '
      '   '
      
        '   sum(if(STR_TO_DATE(concat(SUBSTR(f.BillMonth,1,2),'#39'/01/'#39',SUBS' +
        'TR(f.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR_TO_DATE(concat('#39'01/01/'#39',' +
        'SUBSTR(f.bMonth,3,2)),'#39'%m/%d/%Y'#39') AND '
      
        '          STR_TO_DATE(concat(SUBSTR(f.BillMonth,1,2),'#39'/01/'#39',SUBS' +
        'TR(f.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_TO_DATE(concat(SUBSTR(f.' +
        'bMonth,1,2),'#39'/01/'#39',SUBSTR(f.bMonth,3,2)),'#39'%m/%d/%Y'#39'),'
      '          ifnull(f.Total,0),0)) total,'
      
        '   sum(if(STR_TO_DATE(concat(SUBSTR(f.BillMonth,1,2),'#39'/01/'#39',SUBS' +
        'TR(f.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR_TO_DATE(concat('#39'01/01/'#39',' +
        'SUBSTR(f.bMonth,3,2)),'#39'%m/%d/%Y'#39') AND '
      
        '          STR_TO_DATE(concat(SUBSTR(f.BillMonth,1,2),'#39'/01/'#39',SUBS' +
        'TR(f.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_TO_DATE(concat(SUBSTR(f.' +
        'bMonth,1,2),'#39'/01/'#39',SUBSTR(f.bMonth,3,2)),'#39'%m/%d/%Y'#39'),'
      '          1,0)) as cntr,'
      '   '
      
        '   sum(if(STR_TO_DATE(concat(SUBSTR(f.BillMonth,1,2),'#39'/01/'#39',SUBS' +
        'TR(f.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '          STR_TO_DATE(concat('#39'01/01/'#39',SUBSTR(f.bMonth,3,2)),'#39'%m/' +
        '%d/%Y'#39'),'
      '          ifnull(f.Total,0),0)) total_P,'
      
        '   sum(if(STR_TO_DATE(concat(SUBSTR(f.BillMonth,1,2),'#39'/01/'#39',SUBS' +
        'TR(f.BillMonth,3,2)),'#39'%m/%d/%Y'#39') < '
      
        '          STR_TO_DATE(concat('#39'01/01/'#39',SUBSTR(f.bMonth,3,2)),'#39'%m/' +
        '%d/%Y'#39'),'
      '                       1,0)) as cntr_P,'
      ''
      '   if(f.wratecode='#39'R'#39','#39'Residential'#39','
      '                      if(f.wratecode='#39'L'#39','#39'Low Voltage'#39','
      
        '                                         if(f.wratecode='#39'H'#39','#39'Hig' +
        'h Voltage'#39','#39#39'))) as description,'
      ''
      '   @PF PF,'
      '   @PR PR,'
      '   @XR XR,'
      '   @OC OC,'
      '   @PN PN,'
      ''
      '   concat(if(@PF<>0 OR @PF_P<>0,'#39'PF01 '#39','#39#39'),'
      '          if(@PR<>0 OR @PR_P<>0,concat('#39'\r\n'#39','#39'P.RENT '#39'),'#39#39'),'
      '          if(@XR<>0 OR @XR_P<>0,concat('#39'\r\n'#39','#39'XRR '#39'),'#39#39'),'
      '          if(@PN<>0 OR @PN_P<>0,concat('#39'\r\n'#39','#39'P.NOTE '#39'),'#39#39'),'
      
        '          if(@OC<>0 OR @OC_P<>0,concat('#39'\r\n'#39','#39'OC '#39'),'#39#39')) ocDesc' +
        ','
      ''
      '   concat(if(@PF<>0 OR @PF_P<>0,@PF,'#39#39'),'
      '          if(@PR<>0 OR @PR_P<>0,concat('#39'\r\n'#39',@PR),'#39#39'),'
      '          if(@XR<>0 OR @XR_P<>0,concat('#39'\r\n'#39',@XR),'#39#39'),'
      '          if(@PN<>0 OR @PN_P<>0,concat('#39'\r\n'#39',@PN),'#39#39'),'
      
        '          if(@OC<>0 OR @OC_P<>0,concat('#39'\r\n'#39',@OC),'#39#39')) ocAmount' +
        ','
      '  '
      ''
      '   @VAT_ VAT,'
      '   @UCNPCSD_  UCNPCSD,'
      '   @UCEC_  UCEC,'
      '   @UCME_  UCME,'
      '   @UCNPCSCC_ UCNPCSCC,'
      '   @RPTax_   RPTa,'
      '   @PARec_   PARec,'
      '   @Fitall_  Fitall,'
      '   @ICCS_    ICCS,'
      '   @MCC_     MCC,'
      '   @gram_    gram,'
      '   @VATDist_  VATDist,'
      '   @VATGenCO_  VATGenCO,'
      '   @VATTransCO_  VATTransCO,'
      '   @EVATSystemLoss_  EVATSystemLoss,'
      ''
      '   @PF_P PF_P,'
      '   @PR_P PR_P,'
      '   @XR_P XR_P,'
      '   @OC_P OC_P,'
      '   @PN_P PN_P,'
      ' '
      '   '
      ''
      '   concat(if(@PF_P<>0 OR @PF<>0,@PF_P,'#39#39'),'
      '          if(@PR_P<>0 OR @PR<>0,concat('#39'\r\n'#39',@PR_P),'#39#39'),'
      '          if(@XR_P<>0 OR @XR<>0,concat('#39'\r\n'#39',@XR_P),'#39#39'),'
      '          if(@PN_P<>0 OR @PN<>0,concat('#39'\r\n'#39',@PN_P),'#39#39'),'
      
        '          if(@OC_P<>0 OR @OC<>0,concat('#39'\r\n'#39',@OC_P),'#39#39')) ocAmou' +
        'nt_P,'
      '   '
      ''
      '    @VAT_P  VAT_P,'
      '    @UCNPCSD_P  UCNPCSD_P,'
      '    @UCEC_P   UCEC_P,'
      '    @UCME_P  UCME_P,'
      '    @UCNPCSCC_P  UCNPCSCC_P,'
      '    @RPTax_P    RPTax_P,'
      '    @PARec_P    PARec_P,'
      '    @Fitall_P   Fitall_P,'
      '    @ICCS_P     ICCS_P,'
      '    @MCC_P      MCC_P,'
      '    @gram_P     gram_P,'
      '    @VATDist_P  VATDist_P,'
      '    @VATGenCO_P  VATGenCO_P,'
      '    @VATTransCO_P VATTransCO_P,'
      '    @EVATSystemLoss_P EVATSystemLoss_P,'
      ''
      '    f.bMonth,'
      '    f.cntrG'
      '    '
      ''
      'from'
      '('
      ''
      'SELECT c.Area, '
      '       c.Book, '
      '       c.BillMonth, '
      '       c.TotalBill, '
      '       c.Total,'
      '       1 cntrG,'
      '       m.wratecode,'
      ''
      
        '       @VAT_ := @VAT_+if(STR_TO_DATE(concat(SUBSTR(c.BillMonth,1' +
        ',2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR_TO_DATE(c' +
        'oncat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                         STR_TO_DATE(concat(SUBSTR(c.BillMonth,1' +
        ',2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_TO_DATE(c' +
        'oncat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
      
        '                                                 ifnull(c.VATDis' +
        't,0)+'
      
        '                                                 ifnull(c.VATDis' +
        'tDiscAmt,0)+'
      
        '                                                 ifnull(c.VATGen' +
        'CO,0)+'
      
        '                                                 ifnull(c.VATGen' +
        'CODiscAmt,0)+'
      
        '                                                 ifnull(c.VATTra' +
        'nsCO,0)+'
      
        '                                                 ifnull(c.VATTra' +
        'nsCODiscAmt,0)+'
      
        '                                                 ifnull(c.VATSys' +
        'temLossGenCO,0)+                                '
      
        '                                                 ifnull(c.VATSys' +
        'temLossDiscAmt,0)+'
      
        '                                                 ifnull(c.VATDis' +
        'cAmt,0),0 '
      '                     ) as VAT,'
      ''
      
        '      @VATDist_ := @VATDist_+if(STR_TO_DATE(concat(SUBSTR(c.Bill' +
        'Month,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR_TO' +
        '_DATE(concat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                                STR_TO_DATE(concat(SUBSTR(c.Bill' +
        'Month,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_TO' +
        '_DATE(concat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
      
        '                                                       ifnull(c.' +
        'VATDist,0)+'
      
        '                                                       ifnull(c.' +
        'VATDistDiscAmt,0),0) EVATDist,'
      
        '      @VATGenCO_ := @VATGenCO_+if(STR_TO_DATE(concat(SUBSTR(c.Bi' +
        'llMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR_' +
        'TO_DATE(concat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                                  STR_TO_DATE(concat(SUBSTR(c.Bi' +
        'llMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_' +
        'TO_DATE(concat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
      
        '                                                         ifnull(' +
        'c.VATGenCO,0)+'
      
        '                                                         ifnull(' +
        'c.VATGenCODiscAmt,0),0) EVATGenCO,'
      
        '      @VATTransCO_ := @VATTransCO_+if(STR_TO_DATE(concat(SUBSTR(' +
        'c.BillMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= ' +
        'STR_TO_DATE(concat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                                      STR_TO_DATE(concat(SUBSTR(' +
        'c.BillMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= ' +
        'STR_TO_DATE(concat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
      
        '                                                             ifn' +
        'ull(c.VATTransCO,0)+'
      
        '                                                             ifn' +
        'ull(c.VATTransCODiscAmt,0),0) EVATTransCO,'
      
        '      @EVATSystemLoss_ := @EVATSystemLoss_+if(STR_TO_DATE(concat' +
        '(SUBSTR(c.BillMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/' +
        '%Y'#39') >= STR_TO_DATE(concat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                                              STR_TO_DATE(concat' +
        '(SUBSTR(c.BillMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/' +
        '%Y'#39') <= STR_TO_DATE(concat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
      
        '                                                                ' +
        '     ifnull(c.VATSystemLossGenCO,0)+           '
      
        '                                                                ' +
        '     ifnull(c.VATSystemLossDiscAmt,0),0) EVATSystemLoss,'
      '     '
      '     '
      ''
      
        '       @UCNPCSD_ := @UCNPCSD_+if(STR_TO_DATE(concat(SUBSTR(c.Bil' +
        'lMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR_T' +
        'O_DATE(concat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                                 STR_TO_DATE(concat(SUBSTR(c.Bil' +
        'lMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_T' +
        'O_DATE(concat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
      '                                 ifnull(c.UCNPCSD,0),0) UCNPCSD,'
      
        '       @UCEC_ := @UCEC_+if(STR_TO_DATE(concat(SUBSTR(c.BillMonth' +
        ',1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR_TO_DATE' +
        '(concat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                           STR_TO_DATE(concat(SUBSTR(c.BillMonth' +
        ',1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_TO_DATE' +
        '(concat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
      '                                 ifnull(c.UCEC,0),0) UCEC,'
      
        '       @UCME_ := @UCME_+if(STR_TO_DATE(concat(SUBSTR(c.BillMonth' +
        ',1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR_TO_DATE' +
        '(concat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                           STR_TO_DATE(concat(SUBSTR(c.BillMonth' +
        ',1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_TO_DATE' +
        '(concat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
      '                                 ifnull(c.UCME,0),0) UCME,'
      
        '       @UCNPCSCC_ := @UCNPCSCC_+if(STR_TO_DATE(concat(SUBSTR(c.B' +
        'illMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR' +
        '_TO_DATE(concat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                                 STR_TO_DATE(concat(SUBSTR(c.Bil' +
        'lMonth,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_T' +
        'O_DATE(concat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
      
        '                                 ifnull(c.UCNPCSCC,0),0) UCNPCSC' +
        'C,'
      
        '       @RPTax_ := @RPTax_+if(STR_TO_DATE(concat(SUBSTR(c.BillMon' +
        'th,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR_TO_DA' +
        'TE(concat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                             STR_TO_DATE(concat(SUBSTR(c.BillMon' +
        'th,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_TO_DA' +
        'TE(concat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
      '                                 ifnull(c.RPTax,0),0) RPTax,'
      
        '       @PARec_ := @PARec_+if(STR_TO_DATE(concat(SUBSTR(c.BillMon' +
        'th,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') >= STR_TO_DA' +
        'TE(concat('#39'01/01/'#39',:year),'#39'%m/%d/%Y'#39') AND '
      
        '                             STR_TO_DATE(concat(SUBSTR(c.BillMon' +
        'th,1,2),'#39'/01/'#39',SUBSTR(c.BillMonth,3,2)),'#39'%m/%d/%Y'#39') <= STR_TO_DA' +
        'TE(concat(:month,'#39'/01/'#39',:year),'#39'%m/%d/%Y'#39'),'
