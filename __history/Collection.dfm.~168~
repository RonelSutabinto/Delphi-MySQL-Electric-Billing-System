        Value = 
          'select       '#13#10'     sum(ifnull(Fitall,0)) Fitall,'#13#10'     sum(ifnu' +
          'll(RPTax,0))  RPT,'#13#10'     sum(ifnull(UCNPCSD,0)) UCNPCSCD, '#13#10'    ' +
          ' sum(ifnull(ICCrossSubsidyCharge,0)) ICCS,'#13#10'     sum(ifnull(PARe' +
          'c,0)) PARec,  '#13#10'     sum(ifnull(VATmcc,0)) VATmcc, '#13#10'     sum(if' +
          'null(gram,0)) gram,'#13#10#13#10'     concat(if(sum(if(d.occode='#39'PF01'#39',ifn' +
          'ull(l.amount,0),0))=0,'#39#39','#13#10'                                     ' +
          '                         concat('#39'PF01 '#39',sum(if(d.occode='#39'PF01'#39',i' +
          'fnull(l.amount,0),0)),'#39'\r\n'#39') ),'#13#10'            if(sum(if(d.occode' +
          '='#39'P.RENTAL'#39',ifnull(l.amount,0),0))=0,'#39#39','#13#10'                      ' +
          '                                        concat( '#39'P.RENT '#39',sum(if' +
          '(d.occode='#39'P.RENTAL'#39',ifnull(l.amount,0),0)),'#39'\r\n'#39') ),'#13#10'        ' +
          '    if(sum(if(d.occode='#39'XRR'#39',ifnull(l.amount,0),0))=0,'#39#39','#13#10'     ' +
          '                                                         concat(' +
          ' '#39'XRR '#39',sum(if(d.occode='#39'XRR'#39',ifnull(l.amount,0),0)),'#39'\r\n'#39') ),'#13 +
          #10'            if(sum(if(d.occode='#39'PROM. NOTE'#39',ifnull(l.amount,0),' +
          '0))=0,'#39#39','#13#10'                                                     ' +
          '         concat( '#39'P.NOTE '#39',sum(if(d.occode='#39'PROM. NOTE'#39',ifnull(l' +
          '.amount,0),0)),'#39'\r\n'#39') ),'#13#10'            if(sum(if(d.occode<>'#39'PROM' +
          '. NOTE'#39' AND'#13#10'                         d.occode<>'#39'XRR'#39' AND '#13#10'    ' +
          '                     d.occode<>'#39'P.RENTAL'#39' AND'#13#10'                 ' +
          '        d.occode<>'#39'PF01'#39',ifnull(l.amount,0),0))=0,'#39#39','#13#10'         ' +
          '                                                     concat('#39'OC ' +
          #39',sum(if(d.occode<>'#39'PROM. NOTE'#39' AND'#13#10'                           ' +
          '                                                       d.occode<' +
          '>'#39'XRR'#39' AND '#13#10'                                                   ' +
          '                               d.occode<>'#39'P.RENTAL'#39' AND'#13#10'       ' +
          '                                                                ' +
          '           d.occode<>'#39'PF01'#39',ifnull(l.amount,0),0)) )  )  '#13#10'     ' +
          '      ) breakDownOC,'#13#10'      DATE_FORMAT(:Datepaid , "%c/%e/%Y") ' +
          'PC'#13#10'from collection a'#13#10#13#10'left join ocledger l on l.code = a.Acco' +
          'untcode and'#13#10'                        l.tornumber = a.tellerornum' +
          'ber and'#13#10'                        l.billmonth = a.billmonth  '#13#10'le' +
          'ft join ocdetail d on d.idOCDetail=l.idOCDetail'#13#10#13#10#13#10'where a.dat' +
          'epaid <= :Datepaid and'#13#10'      a.billmonth = :billmonth '#13#10#13#10#13#10#13#10
      end
      item
        Name = 'area'
        Value = 
          'select       '#13#10'     sum(ifnull(Fitall,0)) Fitall,'#13#10'     sum(ifnu' +
          'll(RPTax,0))  RPT,'#13#10'     sum(ifnull(UCNPCSD,0)) UCNPCSCD, '#13#10'    ' +
          ' sum(ifnull(ICCrossSubsidyCharge,0)) ICCS,'#13#10'     sum(ifnull(PARe' +
          'c,0)) PARec,  '#13#10'     sum(ifnull(VATmcc,0)) VATmcc,'#13#10'     sum(ifn' +
          'ull(gram,0)) gram, '#13#10#13#10'     concat(if(sum(if(d.occode='#39'PF01'#39',ifn' +
          'ull(l.amount,0),0))=0,'#39#39','#13#10'                                     ' +
          '                         concat('#39'PF01 '#39',sum(if(d.occode='#39'PF01'#39',i' +
          'fnull(l.amount,0),0)),'#39'\r\n'#39') ),'#13#10'            if(sum(if(d.occode' +
          '='#39'P.RENTAL'#39',ifnull(l.amount,0),0))=0,'#39#39','#13#10'                      ' +
          '                                        concat( '#39'P.RENT '#39',sum(if' +
          '(d.occode='#39'P.RENTAL'#39',ifnull(l.amount,0),0)),'#39'\r\n'#39') ),'#13#10'        ' +
          '    if(sum(if(d.occode='#39'XRR'#39',ifnull(l.amount,0),0))=0,'#39#39','#13#10'     ' +
          '                                                         concat(' +
          ' '#39'XRR '#39',sum(if(d.occode='#39'XRR'#39',ifnull(l.amount,0),0)),'#39'\r\n'#39') ),'#13 +
          #10'            if(sum(if(d.occode='#39'PROM. NOTE'#39',ifnull(l.amount,0),' +
          '0))=0,'#39#39','#13#10'                                                     ' +
          '         concat( '#39'P.NOTE '#39',sum(if(d.occode='#39'PROM. NOTE'#39',ifnull(l' +
          '.amount,0),0)),'#39'\r\n'#39') ),'#13#10'            if(sum(if(d.occode<>'#39'PROM' +
          '. NOTE'#39' AND'#13#10'                         d.occode<>'#39'XRR'#39' AND '#13#10'    ' +
          '                     d.occode<>'#39'P.RENTAL'#39' AND'#13#10'                 ' +
          '        d.occode<>'#39'PF01'#39',ifnull(l.amount,0),0))=0,'#39#39','#13#10'         ' +
          '                                                     concat('#39'OC ' +
          #39',sum(if(d.occode<>'#39'PROM. NOTE'#39' AND'#13#10'                           ' +
          '                                                       d.occode<' +
          '>'#39'XRR'#39' AND '#13#10'                                                   ' +
          '                               d.occode<>'#39'P.RENTAL'#39' AND'#13#10'       ' +
          '                                                                ' +
          '           d.occode<>'#39'PF01'#39',ifnull(l.amount,0),0)) )  )  '#13#10'     ' +
          '      ) breakDownOC,'#13#10'      DATE_FORMAT(:Datepaid , "%c/%e/%Y") ' +
          'PC'#13#10'from collection a'#13#10#13#10'left join ocledger l on l.code = a.Acco' +
          'untcode and'#13#10'                        l.tornumber = a.tellerornum' +
          'ber and'#13#10'                        l.billmonth = a.billmonth  '#13#10'le' +
          'ft join ocdetail d on d.idOCDetail=l.idOCDetail'#13#10#13#10#13#10'where a.dat' +
          'epaid <= :Datepaid and'#13#10'           a.billmonth = :billmonth and'#13 +
          #10'          a.area = :area '#13#10#13#10#13#10
        Active = False
      end>
