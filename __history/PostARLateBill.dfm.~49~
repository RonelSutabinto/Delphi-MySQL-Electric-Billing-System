  object MasterUpdateQuery: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'Update Low_Priority Ignore Master, LateBill set'
      '    Master.FeedBackCode            = LateBill.FeedBackCode,'
      
        '    Master.PreviousReadingDate     = LateBill.PreviousReadingDat' +
        'e,'
      
        '    Master.PresentReadingDate      = LateBill.PresentReadingDate' +
        ','
      
        '    Master.PreviousReadingKWH      = LateBill.PreviousReadingKWH' +
        ','
      '    Master.PresentReadingKWH       = LateBill.PresentReadingKWH,'
      '    Master.Diff                    = LateBill.Diff,'
      '    Master.Demand                  = LateBill.Demand,'
      '    Master.CM                      = '#39'0'#39','
      '    Master.CMMultiplier            = LateBill.CMMultiplier,'
      
        '    Master.CMPreviousReadingKWH    = LateBill.CMPreviousReadingK' +
        'WH,'
      
        '    Master.CMPresentReadingKWH     = LateBill.CMPresentReadingKW' +
        'H,'
      '    Master.CMKilowattHour          = LateBill.CMKilowattHour,'
      '    Master.CMDemand                = LateBill.CMDemand,'
      '    Master.KilowattHour            = LateBill.KilowattHour,'
      '    Master.KilowattUsed            = LateBill.KilowattUsed,'
      '    Master.GenSysCharge            = LateBill.GenSysCharge,'
      '    Master.HostCommCharge          = LateBill.HostCommCharge,'
      '    Master.ForexCharge             = LateBill.ForexCharge,'
      '    Master.TCDemandCharge          = LateBill.TCDemandCharge,'
      
        '    Master.TCTransSystemCharge     = LateBill.TCTransSystemCharg' +
        'e,'
      '    Master.SystemLossCharge        = LateBill.SystemLossCharge,'
      '    Master.DCDemandCharge          = LateBill.DCDemandCharge,'
      
        '    Master.DCDistributionCharge    = LateBill.DCDistributionChar' +
        'ge,'
      '    Master.SCRetCustCharge         = LateBill.SCRetCustCharge,'
      '    Master.SCSupplySysCharge       = LateBill.SCSupplySysCharge,'
      
