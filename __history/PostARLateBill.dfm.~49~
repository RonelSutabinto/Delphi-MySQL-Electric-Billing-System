      '    Master.LifelineDiscSubs        = LateBill.LifelineDiscSubs,'
      '    Master.LoanCondo               = LateBill.LoanCondo,'
      '    Master.TransformerRental       = LateBill.TransformerRental,'
      '    Master.TotalBill               = LateBill.TotalBill,'
      '    Master.BillNumber              = LateBill.BillNumber,'
      '    Master.OEBRNumber              = LateBill.OEBRNumber,'
      '    Master.BillMonth               = LateBill.BillMonth,'
      '    Master.VATDiscAmt              = LateBill.VATDiscAmt, '
      '    Master.VATDistDiscAmt          = LateBill.VATDistDiscAmt,'
      '    Master.VATGenCODiscAmt         = LateBill.VATGenCODiscAmt,'
      '    Master.VATTransCODiscAmt       = LateBill.VATTransCODiscAmt,'
      
        '    Master.VATSystemLossDiscAmt    = LateBill.VATSystemLossDiscA' +
        'mt,'
      '    Master.VAT                     = LateBill.VAT, '
      '    Master.VATDist                 = LateBill.VATDist,'
      '    Master.VATGenCO                = LateBill.VATGenCO,'
      '    Master.VATTransCO              = LateBill.VATTransCO,'
      
        '    Master.VATSystemLossGenCO      = LateBill.VATSystemLossGenCO' +
        ','
      
        '    Master.VATSystemLossTransCO    = LateBill.VATSystemLossTrans' +
        'CO,'
      
        '    Master.PrevYearAdjPowerCost    = LateBill.PrevYearAdjPowerCo' +
        'st,'
      '    Master.SysLossUnderRecov       = LateBill.SysLossUnderRecov,'
      '    Master.MCC                     = LateBill.MCC,'
      '    Master.DiffBillPerKwhr         = LateBill.DiffBillPerKwhr,'
      '    Master.DiffBillPerKW           = LateBill.DiffBillPerKW,'
      '    Master.DiffBillPerCust         = LateBill.DiffBillPerCust,'
      '    Master.RPTax                   = LateBill.RPTax,'
      '    Master.RateOrder               = LateBill.RateOrder,    '
      '    Master.SCDisc                  = LateBill.SCDisc,'
      '    Master.OtherGenRateAdj           = LateBill.OtherGenRateAdj,'
      
        '    Master.OtherTransCostAdj         = LateBill.OtherTransCostAd' +
        'j,'
      
        '    Master.OtherTransDemandCostAdj   = LateBill.OtherTransDemand' +
        'CostAdj,'
      
        '    Master.OtherSystemLossCostAdj    = LateBill.OtherSystemLossC' +
        'ostAdj,'
      
        '    Master.OtherLifelineRateCostAdj  = LateBill.OtherLifelineRat' +
        'eCostAdj,'
      
        '    Master.OtherSeniorCitizenRateAdj = LateBill.OtherSeniorCitiz' +
        'enRateAdj,'
      '    Master.Fitall                    = LateBill.Fitall,'
      '    Master.referenceAtmNo            = LateBill.referenceAtmNo,'
      '    Master.PARec                     = LateBill.PARec,'
      '    Master.VATparec                  = LateBill.VATparec'
      'where Master.code = LateBill.code and'
      '      LateBill.Area like :Area and '
      '      LateBill.Book like :Book and'
      '      LateBill.OEBRNumber <> '#39#39' and'
      
        '      (CONCAT(RIGHT(LateBill.billmonth,2),LEFT(LateBill.billmont' +
        'h,2)) > IFNULL(CONCAT(RIGHT(Master.billmonth,2),LEFT(Master.bill' +
        'month,2)),'#39#39'))'
      
        '      /*(Master.PresentReadingDate < LateBill.PresentReadingDate' +
        ' or '
      '       Master.PresentReadingDate is null)*/')
    FetchRows = 1
    Left = 306
    Top = 349
    ParamData = <
      item
        DataType = ftString
        Name = 'Area'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'Book'
      end>
  end
  object VatLedgerUpdate: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'Replace into VatLedger'
      '( Code,'
      '  Date,'
      '  Document,'
      '  BillMonth,'
      '  Reading,'
      '  KilowattHour,'
      '  Debit,'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
      '  VATGenCODiscAmt,'
      '  VATTransCODiscAmt,'
      '  VATSystemLossDiscAmt,'
      '  VAT,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO )'
      'select '
      '  Code,'
      '  PresentReadingDate,'
      '  OEBRNumber,'
      '  BillMonth,'
      '  PresentReadingKWH,'
      '  KilowattHour,'
      '  VAT+VATDiscAmt,'
      '  VATDiscAmt,'
      '  VATDistDiscAmt,'
      '  VATGenCODiscAmt,'
      '  VATTransCODiscAmt,'
      '  VATSystemLossDiscAmt,'
      '  VAT,'
      '  VATDist,'
      '  VATGenCO,'
      '  VATTransCO,'
      '  VATSystemLossGenCO,'
      '  VATSystemLossTransCO'
      'from LateBill'
      'where (OEBRNumber <> '#39#39') and'
      '      (Area like :Area and'
      '       Book like :Book) and'
      '       Truncate(VATDiscAmt,2) < 0.00')
    Left = 332
    Top = 348
    ParamData = <
      item
        DataType = ftString
        Name = 'Area'
        Value = ''
      end
      item
        DataType = ftString
        Name = 'Book'
        Value = ''
      end>
  end
end
