  object SpotBillQuery: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'Select c.* '
      'from collection c '
      'left join '
      ''
      'where c.billmonth = :billmonth and '
      '      c.OPDescription = '#39'SPOT BILL'#39)
