  object queCall_Q: TMyQuery
    Connection = BillingData.callerServer
    SQL.Strings = (
      ''
      'Select '
      ' f.*'
      'from'
      '('
      ''
      '('
      ' Select '
      '  2 as pri,'
      '  q.* '
      ' from que q '
      ' left join queproceedhistory qh on qh.number= q.number and'
      '                                   qh.type  = q.type and'
      '                                   qh.date  = q.date'
      ' where q.typePrimary = :typePrimary and'
      '       isProceed = 1 and'
      '       qh.user = :user'
      ' order by q.queID asc limit 1'
      ')'
      'union'
      '-- ==========================='
      '-- ==========================='
      '('
      '  Select '
      '   1 as pri,'
      '   q.* '
      '   from '
      '  que q where q.groupno   = :groupno and'
      '              q.type      = :type and'
      '              q.isProceed = 0 or'
      '        '
      '              q.typePrimary = :typePrimary and'
      '              q.isProceed   = 1 and'
      
        '              not exists (Select * from queproceedhistory qh  wh' +
        'ere qh.number = q.number and'
      
        '                                                                ' +
        '    qh.type   = q.type and'
      
        '                                                                ' +
        '    qh.date   = q.date and'
      
        '                                                                ' +
        '    qh.user   = :user)'
      '  order by q.queID asc limit 1'
      ' )'
      '-- ==========================='
      '-- ==========================='
      ') f order by f.pri desc limit 1'
      ''
      ''
      '')
    Left = 184
    Top = 40
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'typePrimary'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'user'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'groupno'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'type'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'typePrimary'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'user'
        Value = nil
      end>
