  object queCntrQ: TMyQuery
    Connection = BillingData.callerServer
    SQL.Strings = (
      'set @bc := 0;'
      'set @bi := 0;'
      'Select '
      ' @bc as billingC, '
      ' @bi as billingI, '
      ' f.flg'
      'from'
      '('
      '(Select'
      ' if(groupno=6 and type='#39'BC'#39',@bc := @bc+1,@bc) as billingC, '
      ' if(groupno=5 and type='#39'BI'#39',@bi := @bi+1,@bi) as billingI, '
      ' '#39#39' as flg'
      ''
      'from que'
      'where isProceed = 0'
      ')'
      'union'
      ''
      '(Select'
      ' if(q.typePrimary='#39'BC'#39',@bc := @bc+1,@bc) as billingC,  '
      ' if(q.typePrimary='#39'BI'#39',@bi := @bi+1,@bi) as billingI, '
      ' '#39#39' as flg'
      'from que q'
      'left join queproceedhistory qh on qh.number=q.number and'
      '                                  qh.type = q.type and'
      '                                  qh.date =q.date'
      'where q.isProceed = 1 and '
      '      qh.user = :user '
      ')'
      ''
      'union'
      '-- ============================'
      '-- ============================'
      '(Select'
      ' if(q.typePrimary ='#39'BC'#39',@bc := @bc+1,@bc) as billingC, '
      ' if(q.typePrimary ='#39'BI'#39',@bi := @bi+1,@bi) as billingI, '
      ' '#39#39' as flg'
      ''
      'from que q'
      'where q.isProceed = 1 and'
      
        '      not exists (Select * from queproceedhistory qh  where qh.n' +
        'umber = q.number and'
      
        '                                                            qh.t' +
        'ype   = q.type and'
      
        '                                                            qh.d' +
        'ate   = q.date and'
      
        '                                                            qh.u' +
        'ser   = :user) '
      ')'
      '-- ============================'
      '-- ============================'
      ''
      'union'
      '(Select'
      ' 0 billingC, '
      ' 0 billingI,'
      ' '#39'H'#39' as flg)'
      ') f'
      'where flg = '#39'H'#39
      '')
    Left = 328
    Top = 40
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'user'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'user'
        Value = nil
      end>
    object queCntrQflg: TStringField
      FieldName = 'flg'
      Size = 1
    end
    object queCntrQbillingC: TLargeintField
      FieldName = 'billingC'
    end
    object queCntrQbillingI: TLargeintField
      FieldName = 'billingI'
    end
  end
