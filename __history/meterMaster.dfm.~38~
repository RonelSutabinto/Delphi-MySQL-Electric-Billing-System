  object meterHistoryQ: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'Select'
      '  f.MeterSerial,'
      '  f.MeterBrand,'
      '  f.LastReading,'
      '  f.LastReadingDate,'
      '  f.Accountnumber,'
      '  f.Name,'
      '  f.Address'
      ''
      'from'
      '('
      '(Select '
      '  a.Serial MeterSerial,'
      '  a.MeterBrand,'
      '  a.LastReading,'
      '  a.LastReadingDate,'
      '  a.Accountnumber,'
      '  a.Name,'
      '  a.Address'
      'from'
      '((Select'
      '  code as entry,'
      '  Serial,'
      '  MeterBrand,'
      '  PresentReadingKWH as LastReading,'
      '  PresentReadingDate as LastReadingDate,'
      '  accountnumber,'
      '  name,'
      '  Address  '
      ' '
      
        'from master where serial is not null  or serial<>'#39#39' or conncode<' +
        '> 5 )'
      'union'
      '( Select '
      '    r.idremovemeter entry,'
      '    r.serialnumber as Serial,'
      '    r.MeterBrand,  '
      '    r.LastReading,'
      '    r.DateRemoved LastReadingDate,'
      '    r.accountnumber,'
      '    r.name,'
      '    m.Address    '
      '  '
      '  from'
      '  removemeter r '
      '  left join master m on m.accountnumber = r.accountnumber'
      
        '  where not  exists (select master.accountnumber from master whe' +
        're master.serial = r.serialnumber) ) '
      '  '
      ') a'
      'where a.Serial = :serial'
      'group by a.serial)'
      ''
      'union'
      ''
      '(Select     '
      '    c.oldserialNumber as MeterSerial,'
      '    c.OldMeterBrand as MeterBrand,  '
      '    c.LastReading,'
      '    c.Date LastReadingDate,'
      '    c.Accountnumber,'
      '    c.Name,    '
      '    m.Address'
      '  from'
      '  changemeter c'
      '  left join master m on m.accountnumber = c.accountnumber'
      'where c.OldserialNumber = :serial)'
      ') f '
      'order by f.LastReadingDate desc')
    Left = 464
    Top = 24
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'serial'
      end
      item
        DataType = ftUnknown
        Name = 'serial'
      end>
  end
  object meterHistoryDS: TMyDataSource
    DataSet = meterHistoryQ
    Left = 552
    Top = 24
  end
