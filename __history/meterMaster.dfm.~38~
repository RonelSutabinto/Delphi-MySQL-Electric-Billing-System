  object meterMasterQ: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'Select '
      '  a.Serial MeterSerial,'
      '  a.MeterBrand,'
      '  a.LastReading,'
      '  a.LasReadingDate,'
      '  a.accountnumber,'
      '  a.name,'
      '  a.Address  '
      'from'
      ''
      '((Select'
      '  code as entry,'
      '  Serial,'
      '  MeterBrand,'
      '  PresentReadingKWH as LastReading,'
      '  PresentReadingDate as LasReadingDate,'
      '  accountnumber,'
      '  name,'
      '  address    '
      ' '
      
        'from master where serial is not null  or serial<>'#39#39' or conncode<' +
        '> 5 )'
      'union'
      '( Select '
      '    r.idremovemeter entry,'
      '    r.serialnumber as Serial,'
      '    r.MeterBrand,  '
      '    r.LastReading,'
      '    r.DateRemoved LasReadingDate,'
      '    r.accountnumber,'
      '    r.name,'
      '    m.address    '
      '  '
      '  from  '
      '  removemeter r '
      '  left join master m on m.accountnumber = r.accountnumber'
      ''
      
        'where not  exists (select master.accountnumber from master where' +
        ' master.serial = r.serialnumber) ) '
      ''
      'union'
      '(Select '
      '    '#39#39' entry,    '
      '    c.oldserialNumber as Serial,'
      '    c.OldMeterBrand as MeterBrand,  '
      '    c.LastReading,'
      '    c.Date LastReadingDate,'
      '    c.Accountnumber,'
      '    c.Name,'
      '    m.address      '
      '  '
      '  from'
      '  changemeter c'
      '  left join master m on m.accountnumber = c.accountnumber'
      
        'where not  exists (select master.accountnumber from master where' +
        ' master.serial = c.oldserialNumber) -- and'
      
        '     -- not  exists (select removemeter.accountnumber from remov' +
        'emeter where removemeter.serialnumber = c.oldserialNumber)'
      ')'
      '  '
      ') a'
      ''
      ''
      'group by serial'
      'order by entry desc')
    Left = 312
    Top = 16
  end
  object meterMasterDS: TMyDataSource
    DataSet = meterMasterQ
    Left = 360
    Top = 16
  end
