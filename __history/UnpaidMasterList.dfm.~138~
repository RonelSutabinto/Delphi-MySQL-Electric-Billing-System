          '======'#13#10#13#10'                                      ifnull(f.UCEC,0)' +
          ' + '#13#10'                                      ifnull(f.UCME,0) + '#13#10 +
          '                                      ifnull(f.UCNPCSCC,0) + '#13#10' ' +
          '                                     ifnull(f.mcc,0) +          ' +
          '                     '#13#10'                                      ifn' +
          'ull(f.ocamount,0)+                    '#13#10'                        ' +
          '            '#13#10'                                      ifnull(f.Fit' +
          'all,0)+'#13#10'                                      ifnull(f.RPTax,0)' +
          '+'#13#10'                                      ifnull(f.UCNPCSD,0)+'#13#10' ' +
          '                                     ifnull(f.ICCrossSubsidyChar' +
          'ge,0)+'#13#10'                                      ifnull(f.PARec,0)+' +
          #13#10'                                      ifnull(f.gram,0)'#13#10'      ' +
          '                         '#13#10'                                    )' +
          ' ) totalNet,'#13#10#13#10'   1 as groupC,'#13#10'   BM,'#13#10#13#10'   '#13#10'     concat(if(@' +
          'PF=0,'#39#39',concat('#39'PF01 '#39',format(@PF,2),'#39'\r\n'#39')),'#13#10'          if(@PR' +
          '=0,'#39#39',concat('#39'P.RENT '#39',format(@PR,2),'#39'\r\n'#39')),'#13#10'          if(@XR' +
          '=0,'#39#39',concat('#39'XRR '#39',format(@XR,2),'#39'\r\n'#39')),'#13#10'          if(@PN=0,' +
          #39#39',concat('#39'P.NOTE '#39',format(@PN,2),'#39'\r\n'#39')),'#13#10'          if(@OC=0,' +
          #39#39',concat('#39'OC '#39',format(@OC,2) ))'#13#10'         ) OC'#13#10'   '#13#10#13#10'from '#13#10'(' +
          #13#10'select '#13#10'       u.area,       '#13#10'       u.amount Total,'#13#10'      ' +
          ' u.VATDist,'#13#10'       u.VATDistDiscAmt,'#13#10'       u.VATGenCO,'#13#10'     ' +
          '  u.VATGenCODiscAmt,'#13#10'       u.VATTransCO,'#13#10'       u.VATTransCOD' +
          'iscAmt,'#13#10'       u.VATSystemLossGenCO,                           ' +
          '     '#13#10'       u.VATSystemLossDiscAmt,'#13#10'       u.VATDiscAmt,     ' +
          ' '#13#10'       u.UCEC,'#13#10'       u.UCME,'#13#10'       u.UCNPCSCC,'#13#10'       u.' +
          'SystemLossCharge,'#13#10'       u.OtherSystemLossCostAdj,     '#13#10'      ' +
          ' '#13#10'       o.amount ocamount,'#13#10'       d.occode,'#13#10'       if(b.mcc ' +
          'is null,l.mcc,b.mcc) mcc,         '#13#10#13#10'     u.Fitall,'#13#10'     u.RPT' +
          'ax,'#13#10'     u.UCNPCSD,'#13#10'     u.ICCrossSubsidyCharge,'#13#10'     u.PARec' +
          ', '#13#10'     u.VATmcc,'#13#10'     u.gram,'#13#10#13#10'     if(u.billmonth=:BMcurr,' +
          #39'curr'#39','#13#10'                          if(u.billmonth=:BM30,'#39'30'#39','#13#10' ' +
          '                                              if(u.billmonth=:BM' +
          '60,'#39'60'#39','#39'>60'#39'))) ageBill,'#13#10#13#10'     if(u.billmonth=:BMcurr,'#39'ABcurr' +
          #39','#13#10'                          if(u.billmonth=:BM30,'#39'AC30'#39','#13#10'    ' +
          '                                           if(u.billmonth=:BM60,' +
          #39'AD60'#39','#39'AE>60'#39'))) ageBillTmp,'#13#10#13#10'     u.KilowattHour,'#13#10'     m.co' +
          'nncode,'#13#10'     :BMcurr BM,'#13#10#13#10'     @PF := @PF+if(d.occode='#39'PF01'#39' ' +
          'and m.conncode<>1 ,ifnull(o.amount,0),0) as PF01,'#13#10'     @PR := @' +
          'PR+if(d.occode='#39'P.RENTAL'#39'  and m.conncode<>1,ifnull(o.amount,0),' +
          '0) as PRENTAL,'#13#10'     @XR := @XR+if(d.occode='#39'XRR'#39'  and m.conncod' +
          'e<>1,ifnull(o.amount,0),0) as XRR,'#13#10'     @PN := @PN+if(d.occode=' +
          #39'PROM. NOTE'#39'  and m.conncode<>1,ifnull(o.amount,0),0) PNOTE,'#13#10'  ' +
          '   @OC := @OC+if(d.occode<>'#39'PROM. NOTE'#39' AND'#13#10'                   ' +
          '    d.occode<>'#39'XRR'#39' AND '#13#10'                       d.occode<>'#39'P.RE' +
          'NTAL'#39' AND'#13#10'                       d.occode<>'#39'PF01'#39'  and m.connco' +
          'de<>1,ifnull(o.amount,0),0) OCcharge     '#13#10'     '#13#10'from unpaidbil' +
          'ls u'#13#10'left join master m on (m.code = u.code)'#13#10'left join billhis' +
          'tory b on (b.code = u.code and b.billmonth = u.billmonth)'#13#10'left ' +
          'join latebillhistory l on (l.code = u.code and l.billmonth = u.b' +
          'illmonth)'#13#10'left join ocledger o on (o.code = u.code and o.billmo' +
          'nth = u.billmonth)'#13#10'left join ocdetail d on d.idOCDetail=o.idOCD' +
          'etail'#13#10#13#10'where  u.area = :area'#13#10#13#10') f where  f.conncode<>1'#13#10#13#10'  ' +
          '  group by f.ageBill'#13#10'    order by f.ageBillTmp asc'#13#10#13#10
      end>
    object RCtmpageBill: TStringField
      FieldName = 'ageBill'
      Size = 4
    end
