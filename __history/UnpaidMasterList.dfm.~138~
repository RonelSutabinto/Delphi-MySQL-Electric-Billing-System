  object OC: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      
        'select description, sum(currentamount) current, sum(arrearsamoun' +
        't) arrears from'
      '('
      'select ms.area,'
      '       ms.conncode,'
      '       m.description,'
      '       d.consumername,'
      '       d.occode,'
      '       u.document,'
      '       u.billmonth,'
      '       if(u.billmonth = :mybillmonth,l.amount,0) currentamount,'
      '       if(u.billmonth <> :mybillmonth,l.amount,0) arrearsamount'
      'from zaneco.ocledger l'
      'join zaneco.ocdetail d on (l.idocdetail = d.idocdetail)'
      'join master ms on (ms.code = l.code)'
      
        'join zaneco.unpaidbills u on (l.code = u.code and l.billmonth = ' +
        'u.billmonth)'
      'left join zaneco.ocmaster m on (m.occode = d.occode) '
      '   where ms.conncode = :conncode and ms.area like :area'
      ') mydata'
      'group by description')
    Left = 432
    Top = 24
    ParamData = <
      item
        DataType = ftString
        Name = 'mybillmonth'
        Value = '0412'
      end
      item
