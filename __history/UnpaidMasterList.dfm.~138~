  object RC: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select unpaidbills.*,'
      '       billhistory.mcc,'
      '       billhistory.ICCrossSubsidyCharge ICCS,      '
      '       sum(ocledger.amount) ocamount,'
      '       date_format(UnpaidBills.Date,'#39'%Y%m'#39') sortdate,'
      '       master.conncode,'
      '       master.serial'
      'from unpaidbills'
      'left join master on (master.code = unpaidbills.code)'
      
        'left join billhistory on (billhistory.code = unpaidbills.code an' +
        'd billhistory.billmonth = unpaidbills.billmonth)'
      
        'left join ocledger on (ocledger.code = unpaidbills.code and ocle' +
        'dger.billmonth = unpaidbills.billmonth)'
      'where &area  '
      '      &accountnumber '
      '      &active'
      '      &disco '
      'group by unpaidbills.code, unpaidbills.billmonth'
      'order by unpaidbills.accountnumber, sortdate')
    Left = 328
    Top = 24
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'area'
        Value = nil
      end>
    MacroData = <
      item
        Name = 'area'
        Value = 'unpaidbills.area like :area'
      end
      item
        Name = 'accountnumber'
        Value = 'unpaidbills.accountnumber between :startaccount and :endaccount'
        Active = False
      end
      item
        Name = 'active'
        Value = 'and master.conncode = '#39'1'#39
        Active = False
      end
      item
        Name = 'disco'
        Value = 'and master.conncode <> '#39'1'#39' '
        Active = False
      end>
