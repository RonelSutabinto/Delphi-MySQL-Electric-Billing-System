  object MasterR: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select * from billhistory'
      'where area = :area and '
      '      book = :book and '
      '      billmonth = :billmonth '
      'order by  accountnumber')
    Left = 576
    Top = 41
    ParamData = <
      item
        DataType = ftString
        Name = 'AREA'
        Value = ''
      end
      item
        DataType = ftString
        Name = 'BOOK'
        Value = ''
      end
      item
        DataType = ftString
        Name = 'billmonth'
        Value = ''
      end>
