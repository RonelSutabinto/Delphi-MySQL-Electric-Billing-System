  object MasterT: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select billhistory.accountnumber,'
      '       billhistory.name,'
      '       billhistory.address,'
      '       billhistory.ratecode,'
      '       billhistory.serial,'
      '       billhistory.billmonth,'
      '       billhistory.kilowatthour'
      'from billhistory'
      'where billhistory.billmonth = :billmonth')
    Left = 545
    Top = 108
    ParamData = <
      item
        DataType = ftString
        Name = 'billmonth'
        Value = '0106'
      end>
  end
  object DiscoBills: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select *, '
      '      date_format('
      '        concat('
      
        '           substring(billmonth from 3 for 2),'#39'/'#39',substring(billm' +
        'onth from 1 for 2),'#39'/01'#39'),'#39'%Y%m'#39') '
      '      sortmonth'
      'from discobills'
      'where datepaid = :datepaid'
      'order by accountnumber, sortmonth')
    Left = 613
    Top = 9
    ParamData = <
      item
        DataType = ftString
        Name = 'datepaid'
        Value = ''
      end>
    object DiscoBillsid: TIntegerField
      FieldName = 'id'
    end
