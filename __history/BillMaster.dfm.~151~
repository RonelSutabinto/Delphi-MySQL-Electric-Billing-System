  object MasterDemand: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select accountnumber, '
      '       name, '
      '       case ratecode '
      '        when '#39'R'#39' then '#39'Residential'#39
      '        when '#39'C'#39' then '#39'Commercial'#39
      '        when '#39'I'#39' then '#39'Industrial'#39
      '        when '#39'P'#39' then '#39'Pub. Bldg.'#39
      '        when '#39'S'#39' then '#39'St. Lights'#39
      '        else '#39#39' '
      '       end '
      '       as consumertype,'
      '       kilowattused, '
      '       totalbill '
      'from billhistory'
      'where billmonth = :billmonth and'
      '      kilowattused > 0'
      'order by accountnumber')
    Left = 580
    Top = 9
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'billmonth'
        Value = nil
      end>
    object MasterDemandaccountnumber: TStringField
      FieldName = 'accountnumber'
      Size = 10
    end
    object MasterDemandname: TStringField
      FieldName = 'name'
      Size = 30
    end
    object MasterDemandkilowattused: TFloatField
      FieldName = 'kilowattused'
    end
    object MasterDemandtotalbill: TFloatField
      FieldName = 'totalbill'
      currency = True
    end
    object MasterDemandconsumertype: TStringField
      FieldName = 'consumertype'
      Size = 13
    end
  end
  object MasterDemandSummary: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select area, '
      '       case ratecode '
      '        when '#39'R'#39' then '#39'1'#39
      '        when '#39'C'#39' then '#39'2'#39
      '        when '#39'I'#39' then '#39'3'#39
      '        when '#39'P'#39' then '#39'4'#39
      '        when '#39'S'#39' then '#39'5'#39
      '        else '#39#39' '
      '       end '
      '       as consumerClass,'
      '       case ratecode '
      '        when '#39'R'#39' then '#39'Residential'#39
      '        when '#39'C'#39' then '#39'Commercial'#39
      '        when '#39'I'#39' then '#39'Industrial'#39
      '        when '#39'P'#39' then '#39'Pub. Bldg.'#39
      '        when '#39'S'#39' then '#39'St.Lights'#39
      '        else '#39#39' '
      '       end '
      '       as consumertype,'
      '       case Area '
      '        when '#39'001'#39' then '#39'RIZAL'#39
      '        when '#39'002'#39' then '#39'SIBUTAD'#39
      '        when '#39'003'#39' then '#39'MUTIA'#39
      '        when '#39'004'#39' then '#39'LA LIBERTAD'#39
      '        when '#39'005'#39' then '#39'SERGIO OSME'#209'A'#39
      '        when '#39'006'#39' then '#39'DAPITAN'#39
      '        when '#39'007'#39' then '#39'PI'#209'AN'#39
      '        when '#39'008'#39' then '#39'POLANCO'#39
      '        when '#39'009'#39' then '#39'DIPOLOG NORTH'#39
      '        when '#39'010'#39' then '#39'DIPOLOG SOUTH'#39
      '        when '#39'011'#39' then '#39'KATIPUNAN'#39
      '        when '#39'012'#39' then '#39'ROXAS'#39
      '        when '#39'013'#39' then '#39'MANUKAN'#39
      '        when '#39'014'#39' then '#39'JOSE DALMAN'#39
      '        when '#39'015'#39' then '#39'SINDANGAN'#39
      '        when '#39'016'#39' then '#39'LEON B. POSTIGO'#39
      '        when '#39'017'#39' then '#39'SIAYAN'#39
      '        when '#39'018'#39' then '#39'SALUG'#39
      '        when '#39'019'#39' then '#39'LILOY'#39
      '        when '#39'020'#39' then '#39'LABASON'#39
      '        when '#39'021'#39' then '#39'TAMPILISAN'#39
      '        when '#39'022'#39' then '#39'GODOD'#39
      '        when '#39'023'#39' then '#39'GUTALAC'#39
      '        when '#39'024'#39' then '#39'DAPITAN / PI'#209'AN'#39
      '        when '#39'025'#39' then '#39'KALAWIT'#39
      '        else '#39#39' '
      '       end '
      '       as address,'
      '       truncate(sum(kilowattused),2) kilowattused, '
      '       truncate(sum(totalbill),2) totalbill'
      'from billhistory'
      
        'where year(concat(substring(billmonth,3,2),'#39'/'#39',substring(billmon' +
        'th,1,2),'#39'/01'#39')) = :Year and'
      '      kilowattused > 0'
      'group by area, consumertype'
      'order by area, consumerclass')
    Left = 649
    Top = 105
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Year'
        Value = nil
      end>
    object MasterDemandSummaryarea: TStringField
      FieldName = 'area'
      FixedChar = True
      Size = 3
    end
    object MasterDemandSummaryconsumerClass: TStringField
      FieldName = 'consumerClass'
      Size = 1
    end
    object MasterDemandSummaryconsumertype: TStringField
      FieldName = 'consumertype'
      Size = 11
    end
    object MasterDemandSummarykilowattused: TFloatField
      FieldName = 'kilowattused'
    end
    object MasterDemandSummarytotalbill: TFloatField
      FieldName = 'totalbill'
      currency = True
    end
    object MasterDemandSummaryaddress: TStringField
      FieldName = 'address'
      Size = 15
    end
  end
