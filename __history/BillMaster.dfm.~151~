    SQL.Strings = (
      'select * from billhistory'
      'where code = :code and'
      
        '      period_diff(date_format(current_date,'#39'%Y%m'#39'),date_format(b' +
        'illhistory.presentreadingdate,'#39'%Y%m'#39')) <= 13'
