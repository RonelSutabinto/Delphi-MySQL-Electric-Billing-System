  object QueryDisco: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      '(select d.*,'
      '       concat(db.collectorcode,'#39' - '#39',c.name) Disconnector'
      'from disconnection d'
      'left join discobills db on d.accountnumber = db.accountnumber'
      'left join collector c on db.collectorcode = c.collectorcode'
      ''
      
        'where  d.`date` = db.`datepaid` and d.accountnumber = :accountnu' +
        'mber'
      'group by d.`date` desc)'
      'union'
      '(Select d.*,'
      '        '#39#39' Disconnector'
      ' from disconnection d'
      ' where d.accountnumber = :accountnumber and'
      
        '       not exists (select * from discobills db where db.code = d' +
        '.code'
      '                            and db.datepaid = d.date)'
      'group by d.`date` desc)')
    Left = 608
    Top = 240
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'accountnumber'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'accountnumber'
        Value = nil
      end>
    object QueryDiscoEntry: TIntegerField
      FieldName = 'Entry'
      Origin = 'Entry'
    end
    object QueryDiscoCode: TFloatField
      FieldName = 'Code'
      Origin = 'Code'
    end
    object QueryDiscoDate: TDateField
      FieldName = 'Date'
      Origin = 'Date'
    end
    object QueryDiscoArea: TStringField
      FieldName = 'Area'
      Origin = 'Area'
      FixedChar = True
      Size = 3
    end
    object QueryDiscoBook: TStringField
      FieldName = 'Book'
      Origin = 'Book'
      FixedChar = True
      Size = 3
    end
    object QueryDiscoSequence: TStringField
      FieldName = 'Sequence'
      Origin = 'Sequence'
      Size = 4
