  object CountDisco: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'select master.accountnumber,'
      '       master.name,'
      '       sum(unpaidbills.amount) amount'
      'from master, unpaidbills'
      'where master.area = :area and'
      '      master.conncode = '#39'4'#39' and'
      '      master.ratecode like :ratecode and'
      '      unpaidbills.code = master.code and'
      '      unpaidbills.date between :datefrom and :dateto'
      'group by master.code')
